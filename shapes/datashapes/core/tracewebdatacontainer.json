{
  "@context": "https://incf.github.io/neuroshapes/contexts/schema.json",
  "@id": "https://bbp.epfl.ch/shapes/dash/tracewebdatacontainer",
  "@type": "Schema",
  "imports": [
    "https://neuroshapes.org/commons/dataset"
  ],
  "shapes": [
    {
      "@id": "https://bbp.epfl.ch/shapes/dash/tracewebdatacontainer/shapes/TraceWebDataContainerShape",
      "@type": "NodeShape",
      "node": "https://neuroshapes.org/commons/dataset/shapes/DatasetShape",

      "and": [
        {
          "node": "nsg:commons/dataset/shapes/DatasetShape"
        },
        {
          "property": [
             {
              "path": "schema:isPartOf",
              "qualifiedValueShape":
                {
                  "class": "bmo:ExperimentalTrace"
                },
              "qualifiedMinCount": 1
            }
          ]
        }
      ],
      "targetClass": "bmo:TraceWebDataContainer"
    }
  ]
}
