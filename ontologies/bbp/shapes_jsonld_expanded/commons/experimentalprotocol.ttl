@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/commons/experimentalprotocol> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/identifier>,
        <https://neuroshapes.org/commons/protocol>,
        <https://neuroshapes.org/commons/quantitativevalue>,
        <https://neuroshapes.org/commons/typedlabeledontologyterm> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/commons/src/main/resources/schemas/neurosciencegraph/commons/experimentalprotocol/v0.1.1.json" ;
    ns2:shapes <https://neuroshapes.org/commons/experimentalprotocol/shapes/ExperimentalProtocolShape>,
        <https://neuroshapes.org/commons/experimentalprotocol/shapes/HadProtocolValueShape>,
        <https://neuroshapes.org/commons/experimentalprotocol/shapes/MaterialsShape> .

<https://neuroshapes.org/commons/experimentalprotocol/shapes/ExperimentalProtocolShape> a ns1:NodeShape ;
    rdfs:label "Protocol shape definition based on protocols.io json schema. The identifier of a protocol should be a valid IRI."^^xsd:string ;
    rdfs:seeAlso <http://apidoc.protocols.io/v3/#reagent-object> ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/protocol/shapes/ProtocolShape> ] [ ns1:property [ ns1:class <http://schema.org/Organization> ;
                        ns1:description "Name of vendor of protocol" ;
                        ns1:name "Vendor" ;
                        ns1:path <https://neuroshapes.org/vendor> ],
                    [ ns1:description "Usable in activity" ;
                        ns1:name "Usable in activity" ;
                        ns1:node <https://neuroshapes.org/commons/typedlabeledontologyterm/shapes/ActivityTypeOntologyTermShape> ;
                        ns1:path <https://neuroshapes.org/activityType> ],
                    [ ns1:description "Materials used in protocol" ;
                        ns1:name "Materials" ;
                        ns1:node <https://neuroshapes.org/commons/experimentalprotocol/shapes/MaterialsShape> ;
                        ns1:path <https://neuroshapes.org/materials> ],
                    [ ns1:datatype xsd:string ;
                        ns1:description "Warning for protocol" ;
                        ns1:name "Warning" ;
                        ns1:path <https://neuroshapes.org/warning> ],
                    [ ns1:datatype xsd:string ;
                        ns1:description "Steps of protocol" ;
                        ns1:name "Steps" ;
                        ns1:path <https://neuroshapes.org/steps> ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI .

<https://neuroshapes.org/commons/experimentalprotocol/shapes/HadProtocolValueShape> a ns1:PropertyShape ;
    rdfs:seeAlso <https://neuroshapes.org/commons/experimentalprotocol/shapes/ProtocolShape> ;
    ns1:class <https://neuroshapes.org/ExperimentalProtocol> ;
    ns1:description "The value of nsg:hadProtocol should be of type nsg:ExperimentalProtocol." ;
    ns1:name "Experimental protocol" ;
    ns1:path <https://neuroshapes.org/hadProtocol> .

<https://neuroshapes.org/commons/experimentalprotocol/shapes/MaterialsShape> a ns1:NodeShape ;
    ns1:property [ ns1:datatype xsd:string ;
            ns1:description "Name of reagent/material used" ;
            ns1:maxCount 1 ;
            ns1:minCount 1 ;
            ns1:name "Reagent name" ;
            ns1:path <https://neuroshapes.org/reagentName> ],
        [ ns1:description "URL of reagent/material" ;
            ns1:maxCount 1 ;
            ns1:name "Reagent URL" ;
            ns1:node <https://neuroshapes.org/commons/identifier/shapes/IdentifierShape> ;
            ns1:path <http://schema.org/identifier> ],
        [ ns1:datatype xsd:string ;
            ns1:description "Stock keeping unit of reagent/material" ;
            ns1:maxCount 1 ;
            ns1:name "Reagent SKU" ;
            ns1:path <http://schema.org/sku> ],
        [ ns1:datatype xsd:string ;
            ns1:description "Linear formula of reagent" ;
            ns1:maxCount 1 ;
            ns1:name "Reagent linear formula" ;
            ns1:path <https://neuroshapes.org/reagentLinearFormula> ],
        [ ns1:description "Molar weight of reagent" ;
            ns1:maxCount 1 ;
            ns1:name "Reagent molar weight" ;
            ns1:node <https://neuroshapes.org/commons/quantitativevalue/shapes/QuantitativeValueShape> ;
            ns1:path <https://neuroshapes.org/reagentMolarWeight> ],
        [ ns1:class <http://schema.org/Organization> ;
            ns1:description "Name of vendor of reagent" ;
            ns1:name "Reagent vendor" ;
            ns1:path <https://neuroshapes.org/reagentVendor> ] .

