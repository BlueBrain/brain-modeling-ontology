@prefix ns1: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/commons/boundingbox> a ns1:Schema ;
    owl:imports <https://neuroshapes.org/commons/unit>,
        <https://neuroshapes.org/commons/vector3d> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/atlas/src/main/resources/schemas/neurosciencegraph/atlas/boundingbox/v0.2.0.json" ;
    ns1:shapes <https://neuroshapes.org/commons/boundingbox/shapes/BoundingBoxShape> .

<https://neuroshapes.org/commons/boundingbox/shapes/BoundingBoxShape> a ns2:NodeShape ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:property [ ns2:description "The first row in the matrix" ;
            ns2:maxCount 1 ;
            ns2:minCount 1 ;
            ns2:name "First Row" ;
            ns2:node <https://neuroshapes.org/commons/vector3d/shapes/Vector3DShape> ;
            ns2:path <https://neuroshapes.org/upperPoint> ],
        [ ns2:description "The second row in the matrix" ;
            ns2:maxCount 1 ;
            ns2:minCount 1 ;
            ns2:name "Second Row" ;
            ns2:node <https://neuroshapes.org/commons/vector3d/shapes/Vector3DShape> ;
            ns2:path <https://neuroshapes.org/lowerPoint> ],
        <https://neuroshapes.org/commons/unit/shapes/WithUnitCodeShape> .

