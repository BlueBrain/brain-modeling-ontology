@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/commons/propertyvalue> a ns3:Schema ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/nexus-schemas/src/main/resources/schemas/nexus/schemaorgsh/propertyvalue/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/commons/propertyvalue/shapes/PropertyValueShape> .

<https://neuroshapes.org/commons/propertyvalue/shapes/PropertyValueShape> a ns1:NodeShape ;
    ns1:property [ ns1:description "The property value." ;
            ns1:maxCount 1 ;
            ns1:name "Value" ;
            ns1:path <http://schema.org/value> ],
        [ ns1:datatype xsd:string ;
            ns1:description "The property name." ;
            ns1:maxCount 1 ;
            ns1:name "Property ID" ;
            ns1:path <http://schema.org/propertyID> ] .

