@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/commons/entity> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/distribution>,
        <https://neuroshapes.org/commons/identifier>,
        <https://neuroshapes.org/commons/language>,
        <https://neuroshapes.org/commons/license>,
        <https://provshapes.org/commons/derivation>,
        <https://provshapes.org/commons/generation>,
        <https://provshapes.org/commons/invalidation> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/commons/src/main/resources/schemas/neurosciencegraph/commons/entity/v1.0.0.json" ;
    ns3:shapes <https://neuroshapes.org/commons/entity/shapes/EntityShape> .

<https://neuroshapes.org/commons/entity/shapes/EntityShape> a ns1:NodeShape ;
    rdfs:label "Entity shape"^^xsd:string ;
    rdfs:comment "An entity is a physical, digital, conceptual, or other kind of thing with some fixed aspects; entities may be real or imaginary."^^xsd:string ;
    ns1:property [ ns1:description "The entity for which this entity is a revision of. An entity revision is a specific type of derivation." ;
            ns1:maxCount 1 ;
            ns1:name "Revision of" ;
            ns1:nodeKind ns1:IRI ;
            ns1:path ns2:wasRevisionOf ],
        [ ns1:description "A citation or reference to another creative work, such as another publication, web page, scholarly article, etc." ;
            ns1:name "Citation" ;
            ns1:or ( [ ns1:datatype xsd:string ] [ ns1:nodeKind ns1:IRI ] ) ;
            ns1:path <http://schema.org/citation> ],
        [ ns1:description "Identifier" ;
            ns1:name "Identifier" ;
            ns1:node <https://neuroshapes.org/commons/identifier/shapes/IdentifierShape> ;
            ns1:path <http://schema.org/identifier> ],
        [ ns1:datatype xsd:string ;
            ns1:description "Keywords describing the entity." ;
            ns1:name "Keywords" ;
            ns1:path <http://schema.org/keywords> ],
        [ rdfs:seeAlso <https://neuroshapes.org/commons/contribution/shapes/ContributionShape> ;
            ns1:class <https://neuroshapes.org/Contribution> ;
            ns1:description "Information about the contribution of an agent during the generation of this dataset." ;
            ns1:name "Qualified Contribution" ;
            ns1:nodeKind ns1:BlankNodeOrIRI ;
            ns1:path <https://neuroshapes.org/contribution> ],
        [ rdfs:seeAlso <https://provshapes.org/commons/invalidation/shapes/InvalidationShape> ;
            ns1:class ns2:Invalidation ;
            ns1:description "Invalidation." ;
            ns1:name "Qualified Invalidation" ;
            ns1:nodeKind ns1:BlankNodeOrIRI ;
            ns1:path <https://neuroshapes.org/invalidation> ],
        [ ns1:datatype xsd:string ;
            ns1:description "The entity name." ;
            ns1:maxCount 1 ;
            ns1:minCount 1 ;
            ns1:name "Name" ;
            ns1:path <http://schema.org/name> ],
        [ rdfs:seeAlso <https://provshapes.org/commons/generation/shapes/GenerationShape> ;
            ns1:class ns2:Generation ;
            ns1:description "Generation." ;
            ns1:name "Qualified Generation" ;
            ns1:nodeKind ns1:BlankNodeOrIRI ;
            ns1:path <https://neuroshapes.org/generation> ],
        [ ns1:description "The dataset language." ;
            ns1:maxCount 1 ;
            ns1:name "Language" ;
            ns1:node <https://neuroshapes.org/commons/language/shapes/LanguageShape> ;
            ns1:path <http://schema.org/language> ],
        [ ns1:description "The entity from which this entity was derived from." ;
            ns1:name "Derived from" ;
            ns1:nodeKind ns1:IRI ;
            ns1:path ns2:wasDerivedFrom ],
        [ ns1:description "A landing page describing the entity." ;
            ns1:name "URL" ;
            ns1:nodeKind ns1:IRI ;
            ns1:path <http://schema.org/url> ],
        [ ns1:description "URI of an entity sharing the same identity." ;
            ns1:name "Same as" ;
            ns1:nodeKind ns1:IRI ;
            ns1:path <http://schema.org/sameAs> ],
        [ rdfs:seeAlso <https://github.com/creativecommons/cc.licenserdf> ;
            ns1:class <https://neuroshapes.org/License> ;
            ns1:description "A Link towards the dataset license." ;
            ns1:name "License" ;
            ns1:node <https://neuroshapes.org/commons/license/shapes/LicenseShape> ;
            ns1:path <http://schema.org/license> ],
        [ ns1:datatype xsd:string ;
            ns1:description "The entity description" ;
            ns1:maxCount 1 ;
            ns1:name "Description" ;
            ns1:path <http://schema.org/description> ],
        [ ns1:description "The date of the last update of this entity." ;
            ns1:maxCount 1 ;
            ns1:name "Update date" ;
            ns1:path <http://schema.org/dateModified> ],
        [ ns1:description "The agent to which this entity was attributed to." ;
            ns1:name "Attributed to" ;
            ns1:nodeKind ns1:IRI ;
            ns1:path ns2:wasAttributedTo ],
        [ ns1:class <http://schema.org/DataDownload> ;
            ns1:description "A dataset in downloadable form. The downloadable form has a specific format and is located at a specific location." ;
            ns1:name "Distribution" ;
            ns1:node <https://neuroshapes.org/commons/distribution/shapes/DistributionShape> ;
            ns1:path <http://schema.org/distribution> ],
        [ rdfs:seeAlso <https://provshapes.org/commons/derivation/shapes/DerivationShape> ;
            ns1:class ns2:Derivation ;
            ns1:description "A derivation is a transformation of an entity into another, an update of an entity resulting in a new one, or the construction of a new entity based on a pre-existing entity." ;
            ns1:name "Qualified Derivation" ;
            ns1:nodeKind ns1:BlankNodeOrIRI ;
            ns1:path <https://neuroshapes.org/derivation> ],
        [ ns1:datatype xsd:dateTime ;
            ns1:description "Date at which this entity was created." ;
            ns1:maxCount 1 ;
            ns1:name "Creation date" ;
            ns1:path <http://schema.org/dateCreated> ],
        [ ns1:description "A reference to an entity representing an image that can be used as preview." ;
            ns1:name "Preview" ;
            ns1:nodeKind ns1:IRI ;
            ns1:path <http://schema.org/image> ] .

