@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/commons/contribution> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/typedlabeledontologyterm> ;
    ns2:shapes <https://neuroshapes.org/commons/contribution/shapes/ContributionShape> .

<https://neuroshapes.org/commons/contribution/shapes/ContributionShape> a ns1:NodeShape ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:property [ ns1:class <http://www.w3.org/ns/prov#Plan> ;
            ns1:node <https://neuroshapes.org/commons/labeledontologyentity/shapes/LabeledOntologyEntityShape> ;
            ns1:path <http://www.w3.org/ns/prov#hadPlan> ],
        [ ns1:class <http://www.w3.org/ns/prov#Role> ;
            ns1:node <https://neuroshapes.org/commons/typedlabeledontologyterm/shapes/RoleOntologyTermShape> ;
            ns1:path <http://www.w3.org/ns/prov#hadRole> ],
        [ ns1:class <http://www.w3.org/ns/prov#Agent> ;
            ns1:maxCount 1 ;
            ns1:minCount 1 ;
            ns1:nodeKind ns1:IRI ;
            ns1:path <http://www.w3.org/ns/prov#agent> ] .

