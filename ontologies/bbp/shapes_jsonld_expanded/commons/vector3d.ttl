@prefix ns1: <http://www.w3.org/ns/prov#> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/commons/vector3d> a ns3:Schema ;
    ns1:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/commons/src/main/resources/schemas/neurosciencegraph/commons/vector3d/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/commons/vector3d/shapes/Vector3DShape> .

<https://neuroshapes.org/commons/vector3d/shapes/Vector3DShape> a ns2:NodeShape ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:property [ ns2:datatype xsd:float ;
            ns2:description "Vector value of Z" ;
            ns2:maxCount 1 ;
            ns2:minCount 1 ;
            ns2:name "Z value" ;
            ns2:path <https://neuroshapes.org/valueZ> ],
        [ ns2:datatype xsd:float ;
            ns2:description "Vector value of Y" ;
            ns2:maxCount 1 ;
            ns2:minCount 1 ;
            ns2:name "Y value" ;
            ns2:path <https://neuroshapes.org/valueY> ],
        [ ns2:datatype xsd:float ;
            ns2:description "Vector value of X" ;
            ns2:maxCount 1 ;
            ns2:minCount 1 ;
            ns2:name "X value" ;
            ns2:path <https://neuroshapes.org/valueX> ] .

