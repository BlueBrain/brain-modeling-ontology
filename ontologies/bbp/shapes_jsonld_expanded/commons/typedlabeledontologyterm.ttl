ns4:Activity rdfs:label "Activity type ontology" ;
    ns3:definingOntology ns3: .

ns3:BiologicalSex rdfs:label "Biological Sex" ;
    rdfs:isDefinedBy <http://bbp.epfl.ch/neurosciencegraph/ontologies/sex> .

ns3:BrainRegion rdfs:label "Brain Region ontology" ;
    ns3:definingOntology <http://bbp.epfl.ch/neurosciencegraph/ontologies/brainregion> .

ns3:CellType rdfs:label "Cell Type Ontology." .

ns3:Species rdfs:label "NCBI species taxonomy ontology" ;
    rdfs:isDefinedBy <http://bbp.epfl.ch/neurosciencegraph/ontologies/speciestaxonomy> .

ns3:Strain rdfs:label "Animal strain" ;
    rdfs:isDefinedBy <http://bbp.epfl.ch/neurosciencegraph/ontologies/strain> .

<https://neuroshapes.org/commons/typedlabeledontologyterm/shapes/ActivityTypeOntologyTermShape> a ns1:NodeShape ;
