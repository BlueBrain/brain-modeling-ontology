@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/commons/experimentalactivity> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/experimentalprotocol>,
        <https://provshapes.org/commons/activity> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/commons/src/main/resources/schemas/neurosciencegraph/commons/experimentalactivity/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/commons/experimentalactivity/shapes/ExperimentalActivityShape> .

<https://neuroshapes.org/commons/experimentalactivity/shapes/ExperimentalActivityShape> a ns1:NodeShape ;
    rdfs:label "Experimental activity description."^^xsd:string ;
    ns1:and ( [ ns1:node <https://provshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ ns1:minCount 1 ;
                        ns1:path ns2:used ],
                    <https://neuroshapes.org/commons/experimentalprotocol/shapes/HadProtocolValueShape> ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI .

