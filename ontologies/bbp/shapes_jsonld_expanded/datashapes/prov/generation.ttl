@prefix ns1: <http://www.w3.org/ns/prov#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://provshapes.org/dash/generation> a ns2:Schema ;
    owl:imports <https://provshapes.org/commons/generation> ;
    ns1:wasDerivedFrom "https://github.com/BlueBrain/nexus-prov/blob/v1.2.0/modules/prov/src/main/resources/schemas/nexus/prov/generation/v1.0.0.json" ;
    ns2:shapes <https://provshapes.org/dash/generation/shapes/GenerationShape> .

<https://provshapes.org/dash/generation/shapes/GenerationShape> a ns3:NodeShape ;
    rdfs:label "Entity generation shape"^^xsd:string ;
    rdfs:comment "An instance of prov:Generation provides additional descriptions about the binary prov:wasGeneratedBy relation from a generated prov:Entity to the prov:Activity that generated it. For example, :cake prov:wasGeneratedBy :baking; prov:qualifiedGeneration [ a prov:Generation; prov:activity :baking; :foo :bar ]. Generation is the completion of production of a new entity by an activity. This entity did not exist before generation and becomes available for usage after this generation."^^xsd:string ;
    ns3:node <https://provshapes.org/commons/generation/shapes/GenerationShape> ;
    ns3:nodeKind ns3:BlankNodeOrIRI ;
    ns3:targetClass ns1:Generation .

