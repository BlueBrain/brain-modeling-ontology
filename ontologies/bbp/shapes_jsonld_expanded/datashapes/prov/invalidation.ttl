@prefix ns1: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://provshapes.org/dash/invalidation> a ns1:Schema ;
    owl:imports <https://provshapes.org/commons/invalidation> ;
    ns1:shapes <https://provshapes.org/dash/invalidation/shapes/InvalidationShape> .

<https://provshapes.org/dash/invalidation/shapes/InvalidationShape> a ns2:NodeShape ;
    rdfs:label "Entity Invalidation shape"^^xsd:string ;
    rdfs:comment "Invalidation is the start of the destruction, cessation, or expiry of an existing entity by an activity. The entity is no longer available for use (or further invalidation) after invalidation. Any generation or usage of an entity precedes its invalidation."^^xsd:string ;
    ns2:node <https://provshapes.org/commons/invalidation/shapes/InvalidationShape> ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:targetClass <http://www.w3.org/ns/prov#Invalidation> .

