@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/annotatedslice> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity>,
        <https://neuroshapes.org/commons/quantitativevalue> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/morphology/src/main/resources/schemas/neurosciencegraph/morphology/annotatedslice/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/annotatedslice/shapes/AnnotatedSliceShape> .

<https://neuroshapes.org/dash/annotatedslice/shapes/AnnotatedSliceShape> a ns1:NodeShape ;
    rdfs:label "Annotated slice shape definition"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ rdfs:seeAlso <https://neuroshapes.org/dash/fixedstainedslice/shapes/FixedStainedSliceShape> ;
                        ns1:class <https://neuroshapes.org/FixedStainedSlice> ;
                        ns1:description "The FixedStainedSlice this AnnotatedSlice is a revision of." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Fixed stained slice" ;
                        ns1:path ns3:wasRevisionOf ],
                    [ ns1:datatype xsd:string ;
                        ns1:description "Comments added by the annotator during the annotation process." ;
                        ns1:maxCount 1 ;
                        ns1:name "Annotator comment" ;
                        ns1:path <https://neuroshapes.org/annotatorComment> ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/labeledcellcollection/shapes/LabeledCellCollectionShape> ;
                        ns1:class <https://neuroshapes.org/LabeledCellCollection> ;
                        ns1:description "The collections of labeled cells." ;
                        ns1:minCount 1 ;
                        ns1:name "Patched cell collection" ;
                        ns1:path <http://schema.org/hasPart> ],
                    [ ns1:description "Angle at which the slice is as defined during the annotation process." ;
                        ns1:maxCount 1 ;
                        ns1:name "Annotation angle" ;
                        ns1:node <https://neuroshapes.org/commons/quantitativevalue/shapes/QuantitativeValueShape> ;
                        ns1:path <https://neuroshapes.org/annotationAngle> ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/AnnotatedSlice> .

