@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/templatevolume> a ns3:Schema ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/atlas/src/main/resources/schemas/neurosciencegraph/atlas/templatevolume/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/dash/templatevolume/shapes/TemplateVolumeShape> .

<https://neuroshapes.org/dash/templatevolume/shapes/TemplateVolumeShape> a ns1:NodeShape ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns1:description "Image volume" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Image Volume" ;
                        ns1:node <https://neuroshapes.org/dash/imagevolume/shapes/ImageVolumeShape> ;
                        ns1:path <https://neuroshapes.org/imageVolume> ],
                    [ ns1:class <https://neuroshapes.org/AtlasSpatialReferenceSystem> ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path <https://neuroshapes.org/isRegisteredIn> ] ] ) ;
    ns1:description "Template volume" ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/TemplateVolume> .

