@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/trace> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity>,
        <https://neuroshapes.org/commons/quantitativevalue>,
        <https://neuroshapes.org/commons/typedlabeledontologyterm>,
        <https://neuroshapes.org/commons/unit> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/electrophysiology/src/main/resources/schemas/neurosciencegraph/electrophysiology/trace/v1.0.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/trace/shapes/TraceShape> .

<https://neuroshapes.org/dash/trace/shapes/TraceShape> a ns1:NodeShape ;
    rdfs:label "Electrophysiology trace shape definition. A trace should have an IRI as identifier"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns1:datatype xsd:integer ;
                        ns1:description "Recording channel of trace" ;
                        ns1:maxCount 1 ;
                        ns1:name "Channel" ;
                        ns1:path <https://neuroshapes.org/channel> ],
                    [ ns1:description "Time step (inverted sampling frequency) of data" ;
                        ns1:maxCount 1 ;
                        ns1:name "Time step" ;
                        ns1:node <https://neuroshapes.org/commons/quantitativevalue/shapes/QuantitativeValueShape> ;
                        ns1:path <https://neuroshapes.org/timeStep> ],
                    <https://neuroshapes.org/commons/unit/shapes/UnitCodePropertyShape> ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/Trace> .

