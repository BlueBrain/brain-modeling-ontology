@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/bluepyefefeatures> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/bluepyefefeatures/v0.1.4.json" ;
    ns2:shapes <https://neuroshapes.org/dash/bluepyefefeatures/shapes/BluePyEfeFeaturesShape> .

<https://neuroshapes.org/dash/bluepyefefeatures/shapes/BluePyEfeFeaturesShape> a ns1:NodeShape ;
    rdfs:label "BluePyEfe output shape"^^xsd:string ;
    rdfs:comment "BluePyEfe extracted features."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns1:class <https://neuroshapes.org/EModelRelease> ;
                        ns1:description "The emodel release for which features were extracted." ;
                        ns1:name "Part of" ;
                        ns1:path <http://schema.org/isPartOf> ],
                    [ ns1:class ns3:Entity ;
                        ns1:description "Extracted features(features.json)" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path <https://neuroshapes.org/features> ],
                    [ ns1:class ns3:Entity ;
                        ns1:description "Holding currents and thresholds per cell from experimental traces(hypamp_threshold.json)" ;
                        ns1:maxCount 1 ;
                        ns1:path <https://neuroshapes.org/hypampThreshold> ],
                    [ ns1:class ns3:Entity ;
                        ns1:description "Experimental protocols which produced traces having extracted features(protocols.json)" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path <https://neuroshapes.org/hadProtocol> ],
                    [ ns1:datatype xsd:string ;
                        ns1:description "Electrical cell type for which features were extracted" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path <https://neuroshapes.org/eType> ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/BluePyEfeFeatures> .

