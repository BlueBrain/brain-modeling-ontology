@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <http://www.w3.org/2004/02/skos/core#> .
@prefix ns4: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/etypefeatureprotocol> a ns4:Schema ;
    owl:imports <https://neuroshapes.org/commons/modelinstance> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/etypefeatureprotocol/v0.1.2.json" ;
    ns4:shapes <https://neuroshapes.org/dash/etypefeatureprotocol/shapes/ETypeFeatureProtocolShape> .

<https://neuroshapes.org/dash/etypefeatureprotocol/shapes/ETypeFeatureProtocolShape> a ns1:NodeShape ;
    rdfs:label "This shape is for e-type extracted features and protocols"^^xsd:string ;
    rdfs:comment "When stimulus starts?"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/modelinstance/shapes/ModelInstanceShape> ] [ ns1:property [ ns3:editorialNote "The node constraint should be added."^^xsd:string ;
                        ns1:class ns2:Collection ;
                        ns1:description "The entities from which this entity was derived from." ;
                        ns1:maxCount 1 ;
                        ns1:name "Was derived from" ;
                        ns1:path ns2:wasDerivedFrom ],
                    [ ns1:class <https://neuroshapes.org/EmodelFeatureGeneration> ;
                        ns1:description "Generation context." ;
                        ns1:maxCount 1 ;
                        ns1:name "Generation" ;
                        ns1:path ns2:qualifiedGeneration ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/ETypeFeatureProtocol> .

