    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/singlecelltracegeneration/v0.2.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/singlecelltracegeneration/shapes/SingleCellTraceGenerationShape> .

<https://neuroshapes.org/dash/singlecelltracegeneration/shapes/SingleCellTraceGenerationShape> a ns1:NodeShape ;
