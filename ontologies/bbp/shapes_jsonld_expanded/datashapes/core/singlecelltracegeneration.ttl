    ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/SingleCellTraceGeneration> .

