@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/person> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/person> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/core/src/main/resources/schemas/neurosciencegraph/core/person/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/dash/person/shapes/PersonShape> .

<https://neuroshapes.org/dash/person/shapes/PersonShape> a ns1:NodeShape ;
    rdfs:label "Schema.org person specification is used as a vocabulary to describe a person."^^xsd:string ;
    ns1:node <https://neuroshapes.org/commons/person/shapes/PersonShape> ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <http://schema.org/Person> .

