@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/brainimaging> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/experiment/src/main/resources/schemas/neurosciencegraph/experiment/brainimaging/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/dash/brainimaging/shapes/BrainImagingShape> .

<https://neuroshapes.org/dash/brainimaging/shapes/BrainImagingShape> a ns1:NodeShape ;
    rdfs:label "Brain imaging shape definition"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ ns1:class <https://neuroshapes.org/ImageStack> ;
                        ns1:description "ImageStack generated by brain imaging." ;
                        ns1:maxCount 1 ;
                        ns1:name "ImageStack" ;
                        ns1:path ns2:generated ],
                    [ ns1:datatype xsd:string ;
                        ns1:description "Method used for brain imaging." ;
                        ns1:maxCount 1 ;
                        ns1:name "Imaging method" ;
                        ns1:path <https://neuroshapes.org/imagingMethod> ],
                    [ ns1:class <https://neuroshapes.org/Subject> ;
                        ns1:description "Subject used for brain imaging." ;
                        ns1:maxCount 1 ;
                        ns1:name "Subject" ;
                        ns1:path ns2:used ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/BrainImaging> .

