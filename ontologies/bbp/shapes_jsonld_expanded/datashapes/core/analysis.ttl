@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/analysis> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/analysis/v0.1.2.json" ;
    ns3:shapes <https://neuroshapes.org/dash/analysis/shapes/AnalysisShape> .

<https://neuroshapes.org/dash/analysis/shapes/AnalysisShape> a ns1:NodeShape ;
    rdfs:label "Model or simulation report analysis shape"^^xsd:string ;
    rdfs:comment "The analysis strategy/protocol is defined in a protocol."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ rdfs:seeAlso <https://neuroshapes.org/dash/modelinstance/shapes/ModelInstanceShape> ;
                        ns1:description "Used model." ;
                        ns1:name "Model" ;
                        ns1:path ns2:used ;
                        ns1:qualifiedMaxCount 1 ;
                        ns1:qualifiedMinCount 1 ;
                        ns1:qualifiedValueShape [ ns1:class <https://neuroshapes.org/ModelInstance> ] ;
                        ns1:qualifiedValueShapesDisjoint true ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/variablereport/shapes/VariableReportShape> ;
                        ns1:description "Used variable report." ;
                        ns1:name "Variable report" ;
                        ns1:path ns2:used ;
                        ns1:qualifiedMaxCount 1 ;
                        ns1:qualifiedMinCount 1 ;
                        ns1:qualifiedValueShape [ ns1:class <https://neuroshapes.org/VariableReport> ] ;
                        ns1:qualifiedValueShapesDisjoint true ],
                    [ ns1:minCount 2 ;
                        ns1:path ns2:used ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/analysisresult/shapes/AnalysisResultShape> ;
                        ns1:class <https://neuroshapes.org/AnalysisResult> ;
                        ns1:description "Generated analysis result." ;
                        ns1:minCount 1 ;
                        ns1:name "Analysis Result" ;
                        ns1:path ns2:generated ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/Analysis> .

