@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <http://www.w3.org/2004/02/skos/core#> .
@prefix ns4: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/emodel> a ns4:Schema ;
    owl:imports <https://neuroshapes.org/commons/modelinstance> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/emodel/v0.1.3.json" ;
    ns4:shapes <https://neuroshapes.org/dash/emodel/shapes/EModelShape> .

<https://neuroshapes.org/dash/emodel/shapes/EModelShape> a ns1:NodeShape ;
    rdfs:label "Emodel shape"^^xsd:string ;
    rdfs:comment "An emodel can be queried by what is being modeled (values of modelOf). Values of modelOf: cell type which encapsulates the following info:brain region, etype,mtype, functional type, species"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/modelinstance/shapes/ModelInstanceShape> ] [ ns1:property [ rdfs:seeAlso <https://neuroshapes.org/dash/subcellularmodel/shapes/SubCellularModelShape> ;
                        ns1:class <https://neuroshapes.org/SubCellularModel> ;
                        ns1:description "SubCellular mechanism" ;
                        ns1:name "SubCellular mechanism" ;
                        ns1:path <https://neuroshapes.org/subCellularMechanism> ],
                    [ ns3:editorialNote "Can an emodel belongs to multiple release ?"^^xsd:string ;
                        ns1:class <https://neuroshapes.org/EModelRelease> ;
                        ns1:description "The emodel release (s) this emodel is part of." ;
                        ns1:name "Part of" ;
                        ns1:path <http://schema.org/isPartOf> ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/emodelscript/shapes/EModelScriptShape> ;
                        ns1:class <https://neuroshapes.org/EModelScript> ;
                        ns1:description "Script defining neuron model, e.g. a Hoc file, or a zip file containing multiple Hoc files." ;
                        ns1:name "Model script" ;
                        ns1:path <https://neuroshapes.org/modelScript> ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/EModel> .

