@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/morphologymesh> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/distribution>,
        <https://neuroshapes.org/commons/entity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/morphology/src/main/resources/schemas/neurosciencegraph/morphology/morphologymesh/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/morphologymesh/shapes/MorphologyMeshDistributionShape>,
        <https://neuroshapes.org/dash/morphologymesh/shapes/MorphologyMeshShape> .

<https://neuroshapes.org/dash/morphologymesh/shapes/MorphologyMeshShape> a ns1:NodeShape ;
    rdfs:label "Morphology mesh Shape."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns1:description "Morphology mesh location." ;
                        ns1:minCount 1 ;
                        ns1:name "Distribution" ;
                        ns1:node <https://neuroshapes.org/dash/morphologymesh/shapes/MorphologyMeshDistributionShape> ;
                        ns1:path <http://schema.org/distribution> ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/MorphologyMesh> .

<https://neuroshapes.org/dash/morphologymesh/shapes/MorphologyMeshDistributionShape> a ns1:NodeShape ;
    rdfs:label "A morphology mesh distribution shape."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/distribution/shapes/DistributionShape> ] [ ns1:property [ ns1:description "Morphology mesh allowed media types" ;
                        ns1:in ( "application/stl" "application/ply" "application/obj" "application/vtk" "application/binary" ) ;
                        ns1:name "Media type" ;
                        ns1:path <http://schema.org/encodingFormat> ] ] ) .

