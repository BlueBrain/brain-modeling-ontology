@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/memodel> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/modelinstance> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/memodel/v0.1.4.json" ;
    ns2:shapes <https://neuroshapes.org/dash/memodel/shapes/MEModelShape> .

<https://neuroshapes.org/dash/memodel/shapes/MEModelShape> a ns1:NodeShape ;
    rdfs:label "This shape is for..."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/modelinstance/shapes/ModelInstanceShape> ] [ ns1:property [ rdfs:seeAlso <https://neuroshapes.org/dash/emodel/shapes/EModelShape> ;
                        ns1:class <https://neuroshapes.org/EModel> ;
                        ns1:description "EModel shape" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "EModel" ;
                        ns1:path <https://neuroshapes.org/eModel> ],
                    [ ns1:class <https://neuroshapes.org/EModelScript> ;
                        ns1:description "EModel script which should be used to instantiante single cell model. Should reference one of the scripts in EModel." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "EModel script" ;
                        ns1:path <https://neuroshapes.org/mainModelScript> ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/morphology/shapes/MorphologyShape> ;
                        ns1:class <https://neuroshapes.org/Morphology> ;
                        ns1:description "Morphology shape." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Morphology" ;
                        ns1:path <https://neuroshapes.org/morphology> ],
                    [ ns1:class <https://neuroshapes.org/MEModelRelease> ;
                        ns1:description "The memodel release this memodel is part of." ;
                        ns1:name "Part of" ;
                        ns1:path <http://schema.org/isPartOf> ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/MEModel> .

