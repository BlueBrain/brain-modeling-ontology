@prefix ns1: <http://www.w3.org/ns/prov#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/experimentalactivity> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/experimentalactivity> ;
    ns1:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/core/src/main/resources/schemas/neurosciencegraph/core/experimentalactivity/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/experimentalactivity/shapes/ActivityShape> .

<https://neuroshapes.org/dash/experimentalactivity/shapes/ActivityShape> a ns3:NodeShape ;
    rdfs:label "Activity description."^^xsd:string ;
    ns3:node <https://neuroshapes.org/commons/experimentalactivity/shapes/ExperimentalActivityShape> ;
    ns3:nodeKind ns3:BlankNodeOrIRI ;
    ns3:targetClass <https://neuroshapes.org/ExperimentalActivity> .

