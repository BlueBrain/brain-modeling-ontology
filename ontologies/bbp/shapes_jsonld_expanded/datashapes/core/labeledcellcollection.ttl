@prefix ns1: <http://www.w3.org/ns/prov#> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<https://neuroshapes.org/dash/labeledcellcollection> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/dash/collection> ;
    ns1:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/morphology/src/main/resources/schemas/neurosciencegraph/morphology/labeledcellcollection/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/dash/labeledcellcollection/shapes/LabeledCellCollectionShape> .

<https://neuroshapes.org/dash/labeledcellcollection/shapes/LabeledCellCollectionShape> a ns2:NodeShape ;
    ns2:and ( [ ns2:node <https://neuroshapes.org/dash/collection/shapes/CollectionShape> ] [ ns2:property [ rdfs:seeAlso <https://neuroshapes.org/dash/labeledcell/shapes/LabeledCellShape> ;
                        ns2:class <https://neuroshapes.org/LabeledCell> ;
                        ns2:description "The labeled cells member(s) of the collection" ;
                        ns2:name "Member" ;
                        ns2:path ns1:hadMember ] ] ) ;
    ns2:description "Labeled cell collection" ;
    ns2:targetClass <https://neuroshapes.org/LabeledCellCollection> .

