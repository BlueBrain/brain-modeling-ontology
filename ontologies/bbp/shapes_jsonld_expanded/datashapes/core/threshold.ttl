@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/2004/02/skos/core#> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix ns4: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/threshold> a ns4:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/threshold/v0.1.1.json" ;
    ns4:shapes <https://neuroshapes.org/dash/threshold/shapes/ThresholdShape> .

<https://neuroshapes.org/dash/threshold/shapes/ThresholdShape> a ns1:NodeShape ;
    rdfs:label "Threshold shape"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns2:editorialNote "The node constraint should be added."^^xsd:string ;
                        ns1:class ns3:Collection ;
                        ns1:description "The entities from which this entity was derived from." ;
                        ns1:maxCount 1 ;
                        ns1:name "Was derived from" ;
                        ns1:path ns3:wasDerivedFrom ],
                    [ ns1:class <https://neuroshapes.org/EmodelFeatureGeneration> ;
                        ns1:description "Generation context." ;
                        ns1:maxCount 1 ;
                        ns1:name "Generation" ;
                        ns1:path ns3:qualifiedGeneration ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/Threshold> .

