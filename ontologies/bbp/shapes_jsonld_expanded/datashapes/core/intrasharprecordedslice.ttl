@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/intrasharprecordedslice> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/brainlocation>,
        <https://neuroshapes.org/commons/entity> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/experiment/src/main/resources/schemas/neurosciencegraph/experiment/intrasharprecordedslice/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/dash/intrasharprecordedslice/shapes/IntraCellularSharpElectrodeRecordedSliceShape> .

<https://neuroshapes.org/dash/intrasharprecordedslice/shapes/IntraCellularSharpElectrodeRecordedSliceShape> a ns1:NodeShape ;
    rdfs:label "Intracellular sharp electrode-recorded slice shape definition"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ rdfs:seeAlso <https://neuroshapes.org/dash/slice/shapes/SliceShape> ;
                        ns1:class <https://neuroshapes.org/Slice> ;
                        ns1:description "The slice this recorded slice is a revision of." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Slice" ;
                        ns1:path ns2:wasRevisionOf ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/intrasharprecordedcellcollection/shapes/IntraCellularSharpElectrodeRecordedCellCollectionShape> ;
                        ns1:class ns2:Collection ;
                        ns1:description "The collections of recorded cells." ;
                        ns1:minCount 1 ;
                        ns1:name "Recorded cells collection" ;
                        ns1:path <http://schema.org/hasPart> ],
                    [ ns1:description "Brain location information of the recorded cell" ;
                        ns1:name "Brain location" ;
                        ns1:node <https://neuroshapes.org/commons/brainlocation/shapes/BrainLocationShape> ;
                        ns1:path <https://neuroshapes.org/brainLocation> ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/IntraCellularSharpElectrodeRecordedSlice> .

