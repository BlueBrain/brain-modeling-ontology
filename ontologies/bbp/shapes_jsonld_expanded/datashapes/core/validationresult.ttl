@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix ns4: <http://www.w3.org/2004/02/skos/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/validationresult> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/validationresult/v0.1.1.json" ;
    ns2:shapes <https://neuroshapes.org/dash/validationresult/shapes/ValidationResultShape> .

<https://neuroshapes.org/dash/validationresult/shapes/ValidationResultShape> a ns1:NodeShape ;
    rdfs:label "Model or simulation report analysis result shape"^^xsd:string ;
    rdfs:comment "The model or simulation report analysis result shape."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns4:editorialNote "Is this a QuantitativeValue (with unit) ?"^^xsd:string ;
                        ns1:description "Validation score." ;
                        ns1:minCount 1 ;
                        ns1:name "Score" ;
                        ns1:or ( [ ns1:datatype xsd:float ] [ ns1:datatype xsd:decimal ] [ ns1:datatype xsd:double ] ) ;
                        ns1:path <https://neuroshapes.org/score> ],
                    [ ns4:editorialNote "Is this a QuantitativeValue (with unit) ?"^^xsd:string ;
                        ns1:description "Validation normalized score (e.g. in [0, 1])." ;
                        ns1:minCount 1 ;
                        ns1:name "Normalized score" ;
                        ns1:or ( [ ns1:datatype xsd:float ] [ ns1:datatype xsd:decimal ] [ ns1:datatype xsd:double ] ) ;
                        ns1:path <https://neuroshapes.org/normalizedScore> ],
                    [ rdfs:seeAlso <https://neuroshapes.org/commons/entity/shapes/EntityShape> ;
                        ns1:class ns3:Entity ;
                        ns1:description "Additional data produced like figures." ;
                        ns1:name "Member" ;
                        ns1:path ns3:hadMember ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/ValidationResult> .

