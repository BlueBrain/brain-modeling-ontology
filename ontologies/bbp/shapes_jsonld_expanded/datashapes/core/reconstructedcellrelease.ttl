@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/reconstructedcellrelease> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/reconstructedcellrelease/v1.0.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/reconstructedcellrelease/shapes/ReconstructedCellReleaseShape> .

<https://neuroshapes.org/dash/reconstructedcellrelease/shapes/ReconstructedCellReleaseShape> a ns1:NodeShape ;
    rdfs:label "Model or simulation report analysis result shape"^^xsd:string ;
    rdfs:comment "The model or simulation report analysis result shape."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns1:class <https://neuroshapes.org/ReconstructedCellRelease> ;
                        ns1:description "Reconstructed cell release this release is a revision of." ;
                        ns1:maxCount 1 ;
                        ns1:name "Reconstructed cell release" ;
                        ns1:path ns3:wasRevisionOf ],
                    [ ns1:class <https://neuroshapes.org/ReconstructedCellReleaseGeneration> ;
                        ns1:description "Reconstructed cell release generation." ;
                        ns1:maxCount 1 ;
                        ns1:name "Reconstructed cell release generation" ;
                        ns1:path ns3:qualifiedGeneration ],
                    [ ns1:class <https://neuroshapes.org/ReconstructedCell> ;
                        ns1:description "Reconstructed cells part of the release." ;
                        ns1:minCount 1 ;
                        ns1:name "Reconstructed cell" ;
                        ns1:path <http://schema.org/hasPart> ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/ReconstructedCellRelease> .

