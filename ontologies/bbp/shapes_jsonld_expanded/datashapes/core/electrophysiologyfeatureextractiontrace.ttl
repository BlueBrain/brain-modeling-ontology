@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/electrophysiologyfeatureextractiontrace> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity>,
        <https://neuroshapes.org/commons/labeledontologyentity>,
        <https://neuroshapes.org/commons/quantitativevalue>,
        <https://neuroshapes.org/commons/subject> ;
    ns2:shapes <https://neuroshapes.org/dash/electrophysiologyfeatureextractiontrace/shapes/ElectrophysiologyFeatureExtractionTraceShape> .

<https://neuroshapes.org/dash/electrophysiologyfeatureextractiontrace/shapes/ElectrophysiologyFeatureExtractionTraceShape> a ns1:NodeShape ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns1:class <https://neuroshapes.org/Trace> ;
                        ns1:description "Trace" ;
                        ns1:name "Trace" ;
                        ns1:path <https://neuroshapes.org/trace> ],
                    [ ns1:class <https://neuroshapes.org/Subject> ;
                        ns1:description "The subject information (e.g. species)." ;
                        ns1:name "Subject" ;
                        ns1:path <https://neuroshapes.org/subject> ],
                    [ ns1:description "The shape of the stimulus" ;
                        ns1:name "Stimulus" ;
                        ns1:node <https://neuroshapes.org/dash/electrophysiologyfeatureextractiontrace/shapes/StimulusShape> ;
                        ns1:path <https://neuroshapes.org/stimulus> ],
                    [ ns1:datatype xsd:string ;
                        ns1:description "The e-model" ;
                        ns1:name "E-model" ;
                        ns1:path <https://neuroshapes.org/eModel> ],
                    [ ns1:class <https://neuroshapes.org/Cell> ;
                        ns1:description "Cell" ;
                        ns1:name "Cell" ;
                        ns1:path <https://neuroshapes.org/cell> ] ] <https://neuroshapes.org/dash/electrophysiologyfeatureextractiontrace/shapes/StimulusShape> ) ;
    ns1:nodeKind ns1:IRI ;
    ns1:targetClass <https://neuroshapes.org/ElectrophysiologyFeatureExtractionTrace> .

<https://neuroshapes.org/dash/electrophysiologyfeatureextractiontrace/shapes/StimulusShape> a ns1:NodeShape ;
    rdfs:label "Stimulus shape definition"^^xsd:string ;
    ns1:property [ ns1:maxCount 1 ;
            ns1:name "Stimulus type" ;
            ns1:node <https://neuroshapes.org/commons/labeledontologyentity/shapes/LabeledOntologyEntityShape> ;
            ns1:path <https://neuroshapes.org/stimulusType> ] .

