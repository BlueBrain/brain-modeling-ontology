@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/ionchannelmechanismrelease> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/distribution>,
        <https://neuroshapes.org/dash/modelrelease>,
        <https://neuroshapes.org/dash/subcellularmodel> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/ionchannelmechanismrelease/v0.1.3.json" ;
    ns3:shapes <https://neuroshapes.org/dash/ionchannelmechanismrelease/shapes/IonChannelMechanismReleaseShape> .

<https://neuroshapes.org/dash/ionchannelmechanismrelease/shapes/IonChannelMechanismReleaseShape> a ns1:NodeShape ;
    rdfs:label "This shape is for..."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/dash/modelrelease/shapes/ModelReleaseShape> ] [ ns1:property [ ns1:description "Ion channel release data location." ;
                        ns1:name "Ion channel release data location" ;
                        ns1:node <https://neuroshapes.org/commons/distribution/shapes/DistributionShape> ;
                        ns1:path <http://schema.org/distribution> ],
                    [ ns1:description "Subcellular models which are part of this release." ;
                        ns1:name "Subcellular model" ;
                        ns1:node <https://neuroshapes.org/dash/subcellularmodel/shapes/SubCellularModelShape> ;
                        ns1:path <https://neuroshapes.org/subCellularModel> ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/IonChannelMechanismRelease> .

