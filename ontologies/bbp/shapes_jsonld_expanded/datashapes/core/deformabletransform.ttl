@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

<https://neuroshapes.org/dash/deformabletransform> a ns2:Schema ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/atlas/src/main/resources/schemas/neurosciencegraph/atlas/deformabletransform/v0.2.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/deformabletransform/shapes/DeformableTransformShape> .

<https://neuroshapes.org/dash/deformabletransform/shapes/DeformableTransformShape> a ns1:NodeShape ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/dash/transform/shapes/TransformShape> ] ) ;
    ns1:description "Deformable transform including parametric and non-parametric transform, typically described by displacement vector field" ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/DeformableTransform>,
        <https://neuroshapes.org/Transform> .

