@prefix ns1: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/wholecellpatchclamp> a ns1:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/experiment/src/main/resources/schemas/neurosciencegraph/experiment/wholecellpatchclamp/v0.1.0.json" ;
    ns1:shapes <https://neuroshapes.org/dash/wholecellpatchclamp/shapes/WholeCellPatchClampShape> .

<https://neuroshapes.org/dash/wholecellpatchclamp/shapes/WholeCellPatchClampShape> a ns2:NodeShape ;
    rdfs:label "Whole cell patch clamp"^^xsd:string ;
    ns2:and ( [ ns2:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns2:property [ rdfs:seeAlso <https://neuroshapes.org/dash/slice/shapes/SliceShape> ;
                        ns2:class <https://neuroshapes.org/SliceCollection> ;
                        ns2:description "The slice collection used" ;
                        ns2:minCount 1 ;
                        ns2:path ns3:used ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/slice/shapes/PatchedSliceShape> ;
                        ns2:class <https://neuroshapes.org/PatchedSlice> ;
                        ns2:description "The patched slice generated" ;
                        ns2:path ns3:generated ] ] ) ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:targetClass <https://neuroshapes.org/WholeCellPatchClamp> .

