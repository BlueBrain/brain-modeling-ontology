@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/tracecollection> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/collection> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/tracecollection/v0.1.2.json" ;
    ns3:shapes <https://neuroshapes.org/dash/tracecollection/shapes/TraceCollectionShape> .

<https://neuroshapes.org/dash/tracecollection/shapes/TraceCollectionShape> a ns1:NodeShape ;
    rdfs:label "Trace collection shape"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/collection/shapes/CollectionShape> ] [ ns1:property [ ns1:class <https://neuroshapes.org/Trace> ;
                        ns1:description "The traces of the collection" ;
                        ns1:name "Member" ;
                        ns1:path ns2:hadMember ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/TraceCollection> .

