@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/morphologydiversification> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/morphologydiversification/v0.2.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/morphologydiversification/shapes/MorphologyDiversificationShape> .

<https://neuroshapes.org/dash/morphologydiversification/shapes/MorphologyDiversificationShape> a ns1:NodeShape ;
    rdfs:label "Morphology diversification shape"^^xsd:string ;
    rdfs:comment "Morphology diversification: repair, scaling, grafting, mix-and-match, cloning."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ ns1:class <https://neuroshapes.org/MorphologyRelease> ;
                        ns1:description "Morphology release generated." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path ns3:generated ],
                    [ ns1:description "Morphology diversification used entities." ;
                        ns1:minCount 1 ;
                        ns1:name "Morphology diversification used entities" ;
                        ns1:nodeKind ns1:IRI ;
                        ns1:path ns3:used ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/MorphologyDiversification> .

