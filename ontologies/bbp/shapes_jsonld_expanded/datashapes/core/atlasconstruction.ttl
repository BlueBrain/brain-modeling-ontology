@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/atlasconstruction> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity>,
        <https://neuroshapes.org/commons/subjectcollection> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/atlas/src/main/resources/schemas/neurosciencegraph/atlas/atlasconstruction/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/atlasconstruction/shapes/AtlasConstructionShape> .

<https://neuroshapes.org/dash/atlasconstruction/shapes/AtlasConstructionShape> a ns1:NodeShape ;
    rdfs:label "Atlas construction activity description."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ ns1:maxCount 3 ;
                        ns1:minCount 2 ;
                        ns1:path ns3:generated ],
                    [ ns1:description "Brain Region of type brain parcellation scheme concept (ILXREPLACE:f0abc840e92a7e2872c8ee89827b6d23) defined in: https://raw.githubusercontent.com/SciCrunch/NIF-Ontology/master/ttl/generated/parcellation.ttl" ;
                        ns1:name "Brain region" ;
                        ns1:node <https://neuroshapes.org/commons/typedlabeledontologyterm/shapes/BrainRegionOntologyTermShape> ;
                        ns1:path <https://neuroshapes.org/brainRegion> ],
                    [ ns1:path ns3:generated ;
                        ns1:qualifiedMaxCount 1 ;
                        ns1:qualifiedValueShape [ ns1:class <https://neuroshapes.org/TemplateImageData> ] ;
                        ns1:qualifiedValueShapesDisjoint true ],
                    [ ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path ns3:used ;
                        ns1:xone ( [ rdfs:seeAlso <https://neuroshapes.org/commons/emptycollection/shapes/EmptyCollectionShape> ;
                                    ns1:class ns3:EmptyCollection ;
                                    ns1:description "An empty collection of subject(s) used to construct the atlas. This empty collection is used when the used subjects are not known but their number is." ] [ rdfs:seeAlso <https://neuroshapes.org/commons/subjectcollection/shapes/SubjectCollectionShape> ;
                                    ns1:class ns3:Collection ;
                                    ns1:description "The subjects collection used to construct the atlas. This collection is used when the used subjects are individually known." ] ) ],
                    [ ns1:path ns3:generated ;
                        ns1:qualifiedMaxCount 1 ;
                        ns1:qualifiedMinCount 1 ;
                        ns1:qualifiedValueShape [ ns1:class <https://neuroshapes.org/ParcellationImageData> ] ;
                        ns1:qualifiedValueShapesDisjoint true ],
                    [ ns1:path ns3:generated ;
                        ns1:qualifiedMaxCount 1 ;
                        ns1:qualifiedMinCount 1 ;
                        ns1:qualifiedValueShape [ ns1:class <https://neuroshapes.org/ParcellationLabel> ] ;
                        ns1:qualifiedValueShapesDisjoint true ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/AtlasConstruction> .

