@prefix ns1: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/reconstructedcellreleaseprocess> a ns1:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/reconstructedcellreleaseprocess/v0.2.0.json" ;
    ns1:shapes <https://neuroshapes.org/dash/reconstructedcellreleaseprocess/shapes/ReconstructedCellReleaseProcessShape> .

<https://neuroshapes.org/dash/reconstructedcellreleaseprocess/shapes/ReconstructedCellReleaseProcessShape> a ns2:NodeShape ;
    rdfs:label "Reconstructed cell release process shape"^^xsd:string ;
    rdfs:comment "Reconstructed cell release process."^^xsd:string ;
    ns2:and ( [ ns2:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns2:property [ ns2:class <https://neuroshapes.org/ReconstructedCellRelease> ;
                        ns2:description "Reconstructed cell release generated." ;
                        ns2:maxCount 1 ;
                        ns2:minCount 1 ;
                        ns2:name "Reconstructed cell release" ;
                        ns2:path ns3:generated ],
                    [ ns2:class <https://neuroshapes.org/ReconstructedCell> ;
                        ns2:description "Reconstructed cell used." ;
                        ns2:minCount 1 ;
                        ns2:name "ReconstructedCell" ;
                        ns2:path ns3:used ] ] ) ;
    ns2:targetClass <https://neuroshapes.org/ReconstructedCellReleaseProcess> .

