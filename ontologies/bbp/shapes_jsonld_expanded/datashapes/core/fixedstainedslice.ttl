@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/fixedstainedslice> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/morphology/src/main/resources/schemas/neurosciencegraph/morphology/fixedstainedslice/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/dash/fixedstainedslice/shapes/FixedStainedSliceShape> .

<https://neuroshapes.org/dash/fixedstainedslice/shapes/FixedStainedSliceShape> a ns1:NodeShape ;
    rdfs:label "Fixed stained slice shape definition"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ rdfs:seeAlso <https://neuroshapes.org/dash/patchedslice/shapes/PatchedSliceShape> ;
                        ns1:class <https://neuroshapes.org/PatchedSlice> ;
                        ns1:description "The patchedSlice this fixedStainedSlice is a revision of" ;
                        ns1:maxCount 1 ;
                        ns1:name "patchedSlice" ;
                        ns1:path ns2:wasRevisionOf ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/FixedStainedSlice> .

