@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/collection> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/collection> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/core/src/main/resources/schemas/neurosciencegraph/core/collection/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/collection/shapes/CollectionShape> .

<https://neuroshapes.org/dash/collection/shapes/CollectionShape> a ns1:NodeShape ;
    rdfs:label "A collection shape."^^xsd:string ;
    ns1:node <https://neuroshapes.org/commons/collection/shapes/CollectionShape> ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass ns3:Collection .

