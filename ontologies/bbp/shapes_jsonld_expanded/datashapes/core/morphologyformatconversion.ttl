@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/morphologyformatconversion> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/morphology/src/main/resources/schemas/neurosciencegraph/morphology/morphologyformatconversion/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/morphologyformatconversion/shapes/ReconstructedCellFormatConversionShape> .

<https://neuroshapes.org/dash/morphologyformatconversion/shapes/ReconstructedCellFormatConversionShape> a ns1:NodeShape ;
    rdfs:label "Reconstructed cell format conversion activities description."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ rdfs:seeAlso <https://neuroshapes.org/dash/reconstructedcell/shapes/ReconstructedCellShape> ;
                        ns1:class <https://neuroshapes.org/ReconstructedCell> ;
                        ns1:description "The original reconstructed cell to convert" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path ns3:used ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/reconstructedcell/shapes/ReconstructedCellShape> ;
                        ns1:class <https://neuroshapes.org/ReconstructedCell> ;
                        ns1:description "The converted reconstructed cell" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path ns3:generated ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/ReconstructedCellFormatConversion> .

