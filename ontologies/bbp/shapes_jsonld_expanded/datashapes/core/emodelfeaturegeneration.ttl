@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix ns4: <http://www.w3.org/2004/02/skos/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/emodelfeaturegeneration> a ns2:Schema ;
    owl:imports <https://provshapes.org/commons/generation> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/emodelfeaturegeneration/v0.1.1.json" ;
    ns2:shapes <https://neuroshapes.org/dash/emodelfeaturegeneration/shapes/EmodelFeatureGenerationShape> .

<https://neuroshapes.org/dash/emodelfeaturegeneration/shapes/EmodelFeatureGenerationShape> a ns1:NodeShape ;
    rdfs:label "Trace generation shape definition"^^xsd:string ;
    ns4:editorialNote "What context information should be added in this generation ?"^^xsd:string ;
    ns1:and ( [ ns1:node <https://provshapes.org/commons/generation/shapes/GenerationShape> ] [ ns1:property [ ns4:editorialNote "The sh:node constraint can be added once the validator support node constraint."^^xsd:string ;
                        ns1:class <https://neuroshapes.org/TraceFeatureExtraction> ;
                        ns1:description "Points at the feature experiment activity that generated the emodelFeature" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Activity" ;
                        ns1:path ns3:activity ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/EmodelFeatureGeneration> .

