@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/intrasharpelectrode> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/experiment/src/main/resources/schemas/neurosciencegraph/experiment/intrasharpelectrode/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/intrasharpelectrode/shapes/IntraCellularSharpElectrodeShape> .

<https://neuroshapes.org/dash/intrasharpelectrode/shapes/IntraCellularSharpElectrodeShape> a ns1:NodeShape ;
    rdfs:label "Intracellular sharp electrode experiment"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ rdfs:seeAlso <https://neuroshapes.org/dash/intrasharprecordedslice/shapes/IntraCellularSharpElectrodeRecordedSliceShape> ;
                        ns1:class <https://neuroshapes.org/IntraCellularSharpElectrodeRecordedSlice> ;
                        ns1:description "The recorded slice generated" ;
                        ns1:minCount 1 ;
                        ns1:path ns3:generated ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/slice/shapes/SliceShape> ;
                        ns1:class <https://neuroshapes.org/Slice> ;
                        ns1:description "The slice used" ;
                        ns1:minCount 1 ;
                        ns1:path ns3:used ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/IntraCellularSharpElectrode> .

