@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/ontology> a ns3:Schema ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/core/src/main/resources/schemas/neurosciencegraph/core/ontology/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/dash/ontology/shapes/OntologyShape> .

<https://neuroshapes.org/dash/ontology/shapes/OntologyShape> a ns1:NodeShape ;
    ns1:nodeKind ns1:IRI ;
    ns1:property [ ns1:description "Imported resources." ;
            ns1:name "Imports" ;
            ns1:path owl:imports ],
        [ ns1:datatype xsd:string ;
            ns1:description "The ontology label." ;
            ns1:maxCount 1 ;
            ns1:minCount 1 ;
            ns1:name "Label" ;
            ns1:path rdfs:label ] ;
    ns1:targetClass owl:Ontology .

