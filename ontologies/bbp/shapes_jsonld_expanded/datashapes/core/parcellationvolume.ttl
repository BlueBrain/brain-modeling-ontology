@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/parcellationvolume> a ns2:Schema ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/atlas/src/main/resources/schemas/neurosciencegraph/atlas/parcellationvolume/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/parcellationvolume/shapes/ParcellationVolumeShape> .

<https://neuroshapes.org/dash/parcellationvolume/shapes/ParcellationVolumeShape> a ns1:NodeShape ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/dash/imagevolume/shapes/ImageVolumeShape> ] [ ns1:property [ ns1:class <https://neuroshapes.org/AtlasSpatialReferenceSystem> ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path <https://neuroshapes.org/isRegisteredIn> ] ] ) ;
    ns1:description "Parcellation volume. Parcellation is an imagevolume" ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/ImageVolume>,
        <https://neuroshapes.org/ParcellationVolume> .

