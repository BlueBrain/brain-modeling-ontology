@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/reconstructioncorrection> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/morphology/src/main/resources/schemas/neurosciencegraph/morphology/reconstructioncorrection/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/reconstructioncorrection/shapes/ReconstructionCorrectionShape> .

<https://neuroshapes.org/dash/reconstructioncorrection/shapes/ReconstructionCorrectionShape> a ns1:NodeShape ;
    rdfs:label "Reconstruction correction activity description."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ ns1:class <https://neuroshapes.org/ReconstructedCell> ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path ns3:generated ],
                    [ ns1:class <https://neuroshapes.org/ReconstructedCell> ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path ns3:used ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/ReconstructionCorrection> .

