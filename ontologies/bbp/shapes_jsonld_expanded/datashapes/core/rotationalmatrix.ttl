@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/rotationalmatrix> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/vector3d> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/atlas/src/main/resources/schemas/neurosciencegraph/atlas/rotationalmatrix/v0.2.0.json" ;
    ns3:shapes <https://neuroshapes.org/dash/rotationalmatrix/shapes/RotationalMatrixShape> .

<https://neuroshapes.org/dash/rotationalmatrix/shapes/RotationalMatrixShape> a ns1:NodeShape ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:property [ ns1:description "The first row in the matrix" ;
            ns1:maxCount 1 ;
            ns1:minCount 1 ;
            ns1:name "First Row" ;
            ns1:node <https://neuroshapes.org/commons/vector3d/shapes/Vector3DShape> ;
            ns1:path <https://neuroshapes.org/firstRow> ],
        [ ns1:description "The second row in the matrix" ;
            ns1:maxCount 1 ;
            ns1:minCount 1 ;
            ns1:name "Second Row" ;
            ns1:node <https://neuroshapes.org/commons/vector3d/shapes/Vector3DShape> ;
            ns1:path <https://neuroshapes.org/secondRow> ],
        [ ns1:description "The third row in the matrix" ;
            ns1:maxCount 1 ;
            ns1:minCount 1 ;
            ns1:name "Third Row" ;
            ns1:node <https://neuroshapes.org/commons/vector3d/shapes/Vector3DShape> ;
            ns1:path <https://neuroshapes.org/thirdRow> ] ;
    ns1:targetClass <https://neuroshapes.org/RotationalMatrix> .

