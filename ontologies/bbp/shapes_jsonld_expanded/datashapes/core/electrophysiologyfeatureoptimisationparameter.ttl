@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/electrophysiologyfeatureoptimisationparameter> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity>,
        <https://neuroshapes.org/commons/quantitativevalue>,
        <https://neuroshapes.org/commons/subject> ;
    ns2:shapes <https://neuroshapes.org/dash/electrophysiologyfeatureoptimisationparameter/shapes/ElectrophysiologyFeatureOptimisationParameterShape>,
        <https://neuroshapes.org/dash/electrophysiologyfeatureoptimisationparameter/shapes/ParameterShape> .

<https://neuroshapes.org/dash/electrophysiologyfeatureoptimisationparameter/shapes/ElectrophysiologyFeatureOptimisationParameterShape> a ns1:NodeShape ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns1:datatype xsd:string ;
                        ns1:description "Cellular location" ;
                        ns1:name "Cellular location" ;
                        ns1:path <https://neuroshapes.org/location> ],
                    [ ns1:datatype xsd:string ;
                        ns1:description "Channel distribution" ;
                        ns1:name "Channel distribution" ;
                        ns1:path <https://neuroshapes.org/channelDistribution> ],
                    [ ns1:datatype xsd:string ;
                        ns1:description "Sub cellular mechanism" ;
                        ns1:name "Sub cellular mechanism" ;
                        ns1:path <https://neuroshapes.org/subCellularMechanism> ],
                    [ ns1:description "Parameter" ;
                        ns1:name "Parameter" ;
                        ns1:node <https://neuroshapes.org/dash/electrophysiologyfeatureoptimisationparameter/shapes/ParameterShape> ;
                        ns1:path <https://neuroshapes.org/parameter> ],
                    [ ns1:datatype xsd:string ;
                        ns1:description "The e-model" ;
                        ns1:name "E-model" ;
                        ns1:path <https://neuroshapes.org/eModel> ],
                    [ ns1:class <https://neuroshapes.org/Subject> ;
                        ns1:description "The subject information (e.g. species)." ;
                        ns1:name "Subject" ;
                        ns1:path <https://neuroshapes.org/subject> ] ] ) ;
    ns1:nodeKind ns1:IRI ;
    ns1:targetClass <https://neuroshapes.org/ElectrophysiologyFeatureOptimisationParameter> .

<https://neuroshapes.org/dash/electrophysiologyfeatureoptimisationparameter/shapes/ParameterShape> a ns1:NodeShape ;
    rdfs:label "Parameter shape definition"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/quantitativevalue/shapes/RangeQuantitativeValueShape> ] [ ns1:property [ ns1:datatype xsd:string ;
                        ns1:description "Parameter name" ;
                        ns1:name "Parameter name" ;
                        ns1:path <http://schema.org/name> ] ] ) .

