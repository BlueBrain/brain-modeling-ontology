@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://bbp.epfl.ch/shapes/dash/workflow> a ns2:Schema ;
    owl:imports <https://bbp.epfl.ch/shapes/commons/workflow> ;
    ns2:shapes <https://bbp.epfl.ch/shapes/dash/workflow/shapes/WorkflowShape> .

<https://bbp.epfl.ch/shapes/dash/workflow/shapes/WorkflowShape> a ns1:NodeShape ;
    rdfs:label "A workflow represents a set of actions or steps intended by one or more agents to achieve some goals. A workflow is a specification or a plan and is instantiated by activities."^^xsd:string ;
    ns1:node <https://bbp.epfl.ch/shapes/commons/workflow/shapes/WorkflowShape> ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://bbp.epfl.ch/ontologies/core/bmo/Workflow> .

