@prefix ns1: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/parameter> a ns1:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity> ;
    ns1:shapes <https://neuroshapes.org/dash/parameter/shapes/ParameterShape> .

<https://neuroshapes.org/dash/parameter/shapes/ParameterShape> a ns2:NodeShape ;
    rdfs:label "Parameter shape definition"^^xsd:string ;
    ns2:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:targetClass <https://neuroshapes.org/Parameter> .

