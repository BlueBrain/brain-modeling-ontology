@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/apicalannotation> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/morphology/src/main/resources/schemas/neurosciencegraph/morphology/apicalannotation/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/dash/apicalannotation/shapes/ApicalAnnotationShape> .

<https://neuroshapes.org/dash/apicalannotation/shapes/ApicalAnnotationShape> a ns1:NodeShape ;
    rdfs:label "Apical annotation shape definition"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ rdfs:seeAlso <https://neuroshapes.org/dash/reconstructedwholebraincell/shapes/ReconstructedWholeBrainCellShape> ;
                        ns1:class <https://neuroshapes.org/ReconstructedWholeBrainCell> ;
                        ns1:description "The original whole brain cell" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path ns2:used ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/reconstructedwholebraincell/shapes/ReconstructedWholeBrainCellShape> ;
                        ns1:class <https://neuroshapes.org/ReconstructedWholeBrainCell> ;
                        ns1:description "The whole brain cell with apical annotated" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:path ns2:generated ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/ApicalAnnotation> .

