@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/morphologyrelease> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/distribution>,
        <https://neuroshapes.org/dash/modelrelease> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/morphologyrelease/v0.1.2.json" ;
    ns2:shapes <https://neuroshapes.org/dash/morphologyrelease/shapes/MorphologyDistributionShape>,
        <https://neuroshapes.org/dash/morphologyrelease/shapes/MorphologyReleaseShape> .

<https://neuroshapes.org/dash/morphologyrelease/shapes/MorphologyReleaseShape> a ns1:NodeShape ;
    rdfs:label "This shape is for describing morphology release."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/dash/modelrelease/shapes/ModelReleaseShape> ] [ ns1:property [ rdfs:seeAlso <https://neuroshapes.org/dash/modelreleaseindex/shapes/ModelReleaseIndexShape> ;
                        ns1:class <https://neuroshapes.org/ModelReleaseIndex> ;
                        ns1:description "Morphology release index file." ;
                        ns1:name "Morphology release index file" ;
                        ns1:path <https://neuroshapes.org/morphologyIndex> ],
                    [ ns1:description "Morphology release location." ;
                        ns1:name "Distribution" ;
                        ns1:node <https://neuroshapes.org/dash/morphologyrelease/shapes/MorphologyDistributionShape> ;
                        ns1:path <http://schema.org/distribution> ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/MorphologyRelease> .

<https://neuroshapes.org/dash/morphologyrelease/shapes/MorphologyDistributionShape> a ns1:NodeShape ;
    rdfs:label "A morphology release distribution shape."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/distribution/shapes/DistributionShape> ] [ ns1:property [ ns1:description "Morphology release allowed media types" ;
                        ns1:in ( "application/swc" "application/neurolucida" "application/h5" "application/neuroml" ) ;
                        ns1:name "Media type" ;
                        ns1:path <http://schema.org/encodingFormat> ] ] ) .

