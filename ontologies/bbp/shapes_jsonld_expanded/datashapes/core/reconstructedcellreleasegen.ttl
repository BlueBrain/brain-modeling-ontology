@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/reconstructedcellreleasegen> a ns2:Schema ;
    owl:imports <https://provshapes.org/commons/generation> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/reconstructedcellreleasegen/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/reconstructedcellreleasegen/shapes/ReconstructedCellReleaseGenerationShape> .

<https://neuroshapes.org/dash/reconstructedcellreleasegen/shapes/ReconstructedCellReleaseGenerationShape> a ns1:NodeShape ;
    rdfs:label "Reconstructed cell release generation shape"^^xsd:string ;
    ns1:and ( [ ns1:node <https://provshapes.org/commons/generation/shapes/GenerationShape> ] [ ns1:property [ ns1:class <https://neuroshapes.org/ReconstructedCellReleaseProcess> ;
                        ns1:description "Reconstructed cell release process activity." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Activity" ;
                        ns1:path ns3:activity ],
                    [ ns1:datatype xsd:string ;
                        ns1:description "Context of reconstructed cell release process generation." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Description" ;
                        ns1:path <http://schema.org/description> ],
                    [ ns1:description "At time." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "At time" ;
                        ns1:path ns3:atTime ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/ReconstructedCellReleaseGeneration> .

