@prefix ns1: <http://www.w3.org/ns/prov#> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/emodelrelease> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/dash/modelrelease> ;
    ns1:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/emodelrelease/v0.1.4.json" ;
    ns3:shapes <https://neuroshapes.org/dash/emodelrelease/shapes/EModelReleaseShape> .

<https://neuroshapes.org/dash/emodelrelease/shapes/EModelReleaseShape> a ns2:NodeShape ;
    rdfs:label "EModel release shape. Output of BluePyOpt"^^xsd:string ;
    ns2:and ( [ ns2:node <https://neuroshapes.org/dash/modelrelease/shapes/ModelReleaseShape> ] [ ns2:property [ rdfs:seeAlso <https://neuroshapes.org/dash/modelreleaseindex/shapes/ModelReleaseIndexShape> ;
                        ns2:class <https://neuroshapes.org/ModelReleaseIndex> ;
                        ns2:description "EModel release index file." ;
                        ns2:name "EModel release index file" ;
                        ns2:path <https://neuroshapes.org/emodelIndex> ] ] ) ;
    ns2:targetClass <https://neuroshapes.org/EModelRelease> .

