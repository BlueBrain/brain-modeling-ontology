@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/tracefeatureextraction> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/tracefeatureextraction/v0.2.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/tracefeatureextraction/shapes/TraceFeatureExtractionShape> .

<https://neuroshapes.org/dash/tracefeatureextraction/shapes/TraceFeatureExtractionShape> a ns1:NodeShape ;
    rdfs:label "This shape is for recording the activity of feature extraction"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ ns1:description "The used BluePyEfe configuration file (s)." ;
                        ns1:name "BluePyEfe configuration" ;
                        ns1:path ns3:used ;
                        ns1:qualifiedMaxCount 1 ;
                        ns1:qualifiedMinCount 1 ;
                        ns1:qualifiedValueShape [ ns1:class <https://neuroshapes.org/Configuration> ] ;
                        ns1:qualifiedValueShapesDisjoint true ],
                    [ ns1:description "Used trace collections." ;
                        ns1:name "Trace collection" ;
                        ns1:path ns3:used ;
                        ns1:qualifiedMaxCount 1 ;
                        ns1:qualifiedMinCount 1 ;
                        ns1:qualifiedValueShape [ ns1:class <https://neuroshapes.org/TraceCollection> ] ;
                        ns1:qualifiedValueShapesDisjoint true ],
                    [ ns1:minCount 2 ;
                        ns1:path ns3:used ],
                    [ ns1:class <https://neuroshapes.org/BluePyEfeFeatures> ;
                        ns1:description "Feature extraction generated entity." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Feature extraction generated entity" ;
                        ns1:path ns3:generated ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/TraceFeatureExtraction> .

