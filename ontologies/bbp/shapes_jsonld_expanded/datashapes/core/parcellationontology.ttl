@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/parcellationontology> a ns2:Schema ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/atlas/src/main/resources/schemas/neurosciencegraph/atlas/parcellationontology/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/parcellationontology/shapes/ParcellationOntologyShape> .

<https://neuroshapes.org/dash/parcellationontology/shapes/ParcellationOntologyShape> a ns1:NodeShape ;
    rdfs:label "Parcellation ontology. TODO: The targeting type definied here needs to correspond to the one of the brain parcellations labels in https://raw.githubusercontent.com/SciCrunch/NIF-Ontology/master/ttl/generated/parcellation.ttl"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns1:datatype xsd:string ;
                        ns1:description "The version of the atlas that defines this parcellation ontology." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Atlas Version" ;
                        ns1:path <https://neuroshapes.org/atlasVersion> ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/ParcellationOntology> .

