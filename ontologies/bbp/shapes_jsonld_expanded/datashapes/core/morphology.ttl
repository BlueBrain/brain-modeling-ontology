@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns4: <http://www.w3.org/2004/02/skos/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/morphology> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/modelinstance> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/morphology/v0.1.4.json" ;
    ns3:shapes <https://neuroshapes.org/dash/morphology/shapes/MorphologyDistributionShape>,
        <https://neuroshapes.org/dash/morphology/shapes/MorphologyShape> .

<https://neuroshapes.org/dash/morphology/shapes/MorphologyShape> a ns1:NodeShape ;
    rdfs:label "This shape is for..."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/modelinstance/shapes/ModelInstanceShape> ] [ ns1:property [ ns4:editorialNote "Can a morphology belongs to multiple release ?"^^xsd:string ;
                        ns1:class <https://neuroshapes.org/MorphologyRelease> ;
                        ns1:description "The morphology release (s) this morphology is part of." ;
                        ns1:name "Part of" ;
                        ns1:path <http://schema.org/isPartOf> ],
                    [ ns1:description "Morphology view in 2D" ;
                        ns1:name "view2d" ;
                        ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ;
                        ns1:path <https://neuroshapes.org/view2d> ],
                    [ ns1:description "" ;
                        ns1:name "Distribution" ;
                        ns1:node <https://neuroshapes.org/dash/morphology/shapes/MorphologyDistributionShape> ;
                        ns1:path <http://schema.org/distribution> ],
                    [ ns1:description "Morphology view in 3D" ;
                        ns1:name "view3d" ;
                        ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ;
                        ns1:path <https://neuroshapes.org/view3d> ],
                    [ ns1:description "Morphological cell type" ;
                        ns1:name "mType" ;
                        ns1:node <https://neuroshapes.org/commons/typedlabeledontologyterm/shapes/MorphologicalCellTypeOntologyTermShape> ;
                        ns1:path <https://neuroshapes.org/mType> ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/Morphology> .

<https://neuroshapes.org/dash/morphology/shapes/MorphologyDistributionShape> a ns1:NodeShape ;
    rdfs:label "This shape is for..."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/distribution/shapes/DistributionShape> ] [ ns1:property [ ns1:description "" ;
                        ns1:in ( "application/swc" "application/neurolucida" "application/h5" "application/neuroml" ) ;
                        ns1:name "Media type" ;
                        ns1:path <http://schema.org/encodingFormat> ] ] ) .

