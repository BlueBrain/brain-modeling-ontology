@prefix ns1: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/protocol> a ns1:Schema ;
    owl:imports <https://neuroshapes.org/commons/protocol> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/core/src/main/resources/schemas/neurosciencegraph/core/protocol/v0.1.5.json" ;
    ns1:shapes <https://neuroshapes.org/dash/protocol/shapes/ProtocolShape> .

<https://neuroshapes.org/dash/protocol/shapes/ProtocolShape> a ns2:NodeShape ;
    rdfs:label "A shape for describing a nsg:Protocol instance"^^xsd:string ;
    ns2:node <https://neuroshapes.org/commons/protocol/shapes/ProtocolShape> ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:targetClass <https://neuroshapes.org/Protocol> .

