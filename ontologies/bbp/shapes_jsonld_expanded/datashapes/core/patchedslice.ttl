@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/patchedslice> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/brainlocation>,
        <https://neuroshapes.org/commons/entity>,
        <https://neuroshapes.org/commons/quantitativevalue> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/experiment/src/main/resources/schemas/neurosciencegraph/experiment/patchedslice/v0.1.1.json" ;
    ns3:shapes <https://neuroshapes.org/dash/patchedslice/shapes/PatchedSliceShape> .

<https://neuroshapes.org/dash/patchedslice/shapes/PatchedSliceShape> a ns1:NodeShape ;
    rdfs:label "Patched slice shape definition"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns1:description "Recording temperature" ;
                        ns1:maxCount 1 ;
                        ns1:name "Temperature" ;
                        ns1:node <https://neuroshapes.org/commons/quantitativevalue/shapes/QuantitativeValueShape> ;
                        ns1:path <https://neuroshapes.org/temperature> ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/patchedcellcollection/shapes/PatchedCellCollectionShape> ;
                        ns1:class ns2:Collection ;
                        ns1:description "The collection of patched cells." ;
                        ns1:name "Patched cells collection" ;
                        ns1:path <http://schema.org/hasPart> ],
                    [ ns1:description "Brain location information of the patchedcell" ;
                        ns1:name "Brain location" ;
                        ns1:node <https://neuroshapes.org/commons/brainlocation/shapes/BrainLocationShape> ;
                        ns1:path <https://neuroshapes.org/brainLocation> ],
                    [ ns1:datatype xsd:string ;
                        ns1:description "Solution used in the bath while recording" ;
                        ns1:maxCount 1 ;
                        ns1:name "Bath solution" ;
                        ns1:path <https://neuroshapes.org/bathSolution> ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/slice/shapes/SliceShape> ;
                        ns1:class <https://neuroshapes.org/Slice> ;
                        ns1:description "The slice this patchedSlice is a revision of." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Slice" ;
                        ns1:path ns2:wasRevisionOf ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/PatchedSlice> .

