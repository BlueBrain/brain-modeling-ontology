@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/emodelbuilding> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity>,
        <https://neuroshapes.org/commons/quantitativevalue> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/emodelbuilding/v0.2.5.json" ;
    ns3:shapes <https://neuroshapes.org/dash/emodelbuilding/shapes/EModelBuildingShape> .

<https://neuroshapes.org/dash/emodelbuilding/shapes/EModelBuildingShape> a ns1:NodeShape ;
    rdfs:label "Emodel building shape"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ ns1:description "Best score." ;
                        ns1:maxCount 1 ;
                        ns1:name "Best score" ;
                        ns1:node <https://neuroshapes.org/commons/quantitativevalue/shapes/QuantitativeValueShape> ;
                        ns1:path <https://neuroshapes.org/bestScore> ],
                    [ ns1:class <https://neuroshapes.org/ReconstructedCell> ;
                        ns1:description "Morphology which was used to fit model parameters." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Used" ;
                        ns1:path ns2:used ],
                    [ ns1:class <https://neuroshapes.org/EModel> ;
                        ns1:description "The generated Emodel" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Generated" ;
                        ns1:path ns2:generated ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/EModelBuilding> .

