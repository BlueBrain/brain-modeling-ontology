@prefix ns1: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

<https://neuroshapes.org/dash/modelreleaseindex> a ns1:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/modelreleaseindex/v0.1.2.json" ;
    ns1:shapes <https://neuroshapes.org/dash/modelreleaseindex/shapes/ModelReleaseIndexShape> .

<https://neuroshapes.org/dash/modelreleaseindex/shapes/ModelReleaseIndexShape> a ns2:NodeShape ;
    ns2:and ( [ ns2:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] ) ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:targetClass <https://neuroshapes.org/ModelReleaseIndex> .

