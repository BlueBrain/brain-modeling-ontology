@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/cellcounting> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/activity>,
        <https://neuroshapes.org/commons/brainlocation> ;
    ns2:shapes <https://neuroshapes.org/dash/cellcounting/shapes/CellCountingShape> .

<https://neuroshapes.org/dash/cellcounting/shapes/CellCountingShape> a ns1:NodeShape ;
    rdfs:label "Cell counting shape definition"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/activity/shapes/ActivityShape> ] [ ns1:property [ ns1:description "Brain location information" ;
                        ns1:name "Brain location" ;
                        ns1:node <https://neuroshapes.org/commons/brainlocation/shapes/BrainLocationShape> ;
                        ns1:path <https://neuroshapes.org/brainLocation> ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/CellCounting> .

