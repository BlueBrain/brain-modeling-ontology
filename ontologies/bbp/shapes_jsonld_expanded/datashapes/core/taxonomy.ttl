@prefix ns1: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<https://neuroshapes.org/dash/taxonomy> a ns1:Schema ;
    owl:imports <https://neuroshapes.org/dash/ontology> ;
    ns1:shapes <https://neuroshapes.org/dash/taxonomy/shapes/TaxonomyShape> .

<https://neuroshapes.org/dash/taxonomy/shapes/TaxonomyShape> a ns2:NodeShape ;
    ns2:node <https://neuroshapes.org/dash/ontology/shapes/OntologyShape> ;
    ns2:targetClass owl:Ontology,
        <http://www.w3.org/2004/02/skos/core#ConceptScheme> .

