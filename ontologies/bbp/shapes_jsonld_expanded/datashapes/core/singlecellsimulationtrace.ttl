@prefix ns1: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

<https://neuroshapes.org/dash/singlecellsimulationtrace> a ns1:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/singlecellsimulationtrace/v0.1.4.json" ;
    ns1:shapes <https://neuroshapes.org/dash/singlecellsimulationtrace/shapes/SingleCellSimulationTraceShape> .

<https://neuroshapes.org/dash/singlecellsimulationtrace/shapes/SingleCellSimulationTraceShape> a ns2:NodeShape ;
    ns2:and ( [ ns2:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns2:property [ ns2:description "Preview of the simulation trace as an image" ;
                        ns2:name "image" ;
                        ns2:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ;
                        ns2:path <http://schema.org/image> ] ] ) ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:targetClass <https://neuroshapes.org/SingleCellSimulationTrace> .

