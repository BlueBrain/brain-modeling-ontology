@prefix ns1: <http://www.w3.org/ns/prov#> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

<https://neuroshapes.org/dash/lineartransform> a ns3:Schema ;
    ns1:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/atlas/src/main/resources/schemas/neurosciencegraph/atlas/lineartransform/v0.2.0.json" ;
    ns3:shapes <https://neuroshapes.org/dash/lineartransform/shapes/LinearTransformShape> .

<https://neuroshapes.org/dash/lineartransform/shapes/LinearTransformShape> a ns2:NodeShape ;
    ns2:and ( [ ns2:node <https://neuroshapes.org/dash/transform/shapes/TransformShape> ] ) ;
    ns2:description "Linear transform including rigid and affine transform, described by a 3x4 matrix" ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:targetClass <https://neuroshapes.org/AffineLinearTransform>,
        <https://neuroshapes.org/LinearTransform>,
        <https://neuroshapes.org/RigidLinearTransform>,
        <https://neuroshapes.org/Transform> .

