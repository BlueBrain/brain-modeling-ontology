@prefix ns1: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/organization> a ns1:Schema ;
    owl:imports <https://neuroshapes.org/commons/organization> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/core/src/main/resources/schemas/neurosciencegraph/core/organization/v0.1.0.json" ;
    ns1:shapes <https://neuroshapes.org/dash/organization/shapes/OrganizationShape> .

<https://neuroshapes.org/dash/organization/shapes/OrganizationShape> a ns2:NodeShape ;
    rdfs:label "A shape for describing an organization based on http://schema.org/Organization."^^xsd:string ;
    rdfs:comment "A schema:Organization can have a GRID identifier."^^xsd:string ;
    rdfs:seeAlso <http://schema.org/Organization>,
        <https://www.grid.ac> ;
    ns2:node <https://neuroshapes.org/commons/organization/shapes/OrganizationShape> ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:targetClass <http://schema.org/Organization> .

