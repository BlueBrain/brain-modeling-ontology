@prefix ns1: <http://www.w3.org/ns/prov#> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/target> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity> ;
    ns1:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/target/v0.1.1.json" ;
    ns3:shapes <https://neuroshapes.org/dash/target/shapes/TargetShape> .

<https://neuroshapes.org/dash/target/shapes/TargetShape> a ns2:NodeShape ;
    rdfs:label "Target shape"^^xsd:string ;
    ns2:and ( [ ns2:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] ) ;
    ns2:targetClass <https://neuroshapes.org/Target> .

