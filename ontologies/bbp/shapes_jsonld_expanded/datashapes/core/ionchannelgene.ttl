@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <http://www.w3.org/ns/prov#> .
@prefix ns3: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/ionchannelgene> a ns3:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity>,
        <https://neuroshapes.org/commons/typedlabeledontologyterm> ;
    ns2:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/experiment/src/main/resources/schemas/neurosciencegraph/experiment/ionchannelgene/v0.1.0.json" ;
    ns3:shapes <https://neuroshapes.org/dash/ionchannelgene/shapes/IonChannelGeneShape> .

<https://neuroshapes.org/dash/ionchannelgene/shapes/IonChannelGeneShape> a ns1:NodeShape ;
    rdfs:label "Ion channel gene shape definition"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns1:description "The ion channel gene. Ion channel genes should come fom the gene ontology: http://www.geneontology.org/" ;
                        ns1:maxCount 1 ;
                        ns1:name "Ion channel gene" ;
                        ns1:node <https://neuroshapes.org/commons/typedlabeledontologyterm/shapes/GeneOntologyTermShape> ;
                        ns1:path <https://neuroshapes.org/ionChannelGene> ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/IonChannelGene> .

