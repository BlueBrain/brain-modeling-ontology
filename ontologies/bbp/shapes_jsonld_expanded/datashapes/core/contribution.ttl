@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<https://neuroshapes.org/dash/contribution> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/contribution> ;
    ns2:shapes <https://neuroshapes.org/dash/contribution/shapes/ContributionShape> .

<https://neuroshapes.org/dash/contribution/shapes/ContributionShape> a ns1:NodeShape ;
    ns1:node <https://neuroshapes.org/commons/contribution/shapes/ContributionShape> ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/Contribution> .

