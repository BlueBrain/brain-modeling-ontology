@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/geneexpressionvolumetricdatalayer> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/dash/volumetricdatalayer> ;
    ns2:shapes <https://neuroshapes.org/dash/geneexpressionvolumetricdatalayer/shapes/GeneExpressionVolumetricDataLayerShape> .

<https://neuroshapes.org/dash/geneexpressionvolumetricdatalayer/shapes/GeneExpressionVolumetricDataLayerShape> a ns1:NodeShape ;
    rdfs:label "Data shape definition for volumetric brain data that characterizes gene expression"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/dash/volumetricdatalayer/shapes/VolumetricDataLayer> ] [ ns1:property [ ns1:description "Name of the gene being expressed in this volumetric dataset" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Gene" ;
                        ns1:node <https://neuroshapes.org/commons/labeledontologyentity/shapes/LabeledOntologyEntityShape> ;
                        ns1:path <https://neuroshapes.org/gene> ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/GeneExpressionVolumetricDataLayer>,
        <https://neuroshapes.org/VolumetricDataLayer> .

