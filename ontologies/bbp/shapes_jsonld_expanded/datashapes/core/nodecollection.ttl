@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/2004/02/skos/core#> .
@prefix ns4: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/nodecollection> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity> ;
    ns4:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/nodecollection/v0.1.2.json" ;
    ns2:shapes <https://neuroshapes.org/dash/nodecollection/shapes/NodeCollectionShape> .

<https://neuroshapes.org/dash/nodecollection/shapes/NodeCollectionShape> a ns1:NodeShape ;
    rdfs:label "elements of the collection defined like this: "^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ rdfs:seeAlso <https://neuroshapes.org/dash/memodelrelease/shapes/MEModelReleaseShape> ;
                        ns1:class <https://neuroshapes.org/MEModelRelease> ;
                        ns1:description "MEModel release." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "MEModel release" ;
                        ns1:path <https://neuroshapes.org/memodelRelease> ],
                    [ rdfs:seeAlso <https://neuroshapes.org/dash/circuitcellproperties/shapes/CircuitCellPropertiesShape> ;
                        ns3:editorialNote "BluePyOpt needs to deal with the cell placement mvd3 file."^^xsd:string ;
                        ns1:class <https://neuroshapes.org/CircuitCellProperties> ;
                        ns1:description "Location of circuit file containing general circuit geometry: cell placement, orientation, mvd3." ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Properties" ;
                        ns1:path <https://neuroshapes.org/circuitCellProperties> ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/NodeCollection> .

