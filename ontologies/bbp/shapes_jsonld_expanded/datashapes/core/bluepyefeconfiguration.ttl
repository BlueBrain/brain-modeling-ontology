@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/bluepyefeconfiguration> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/entity>,
        <https://neuroshapes.org/commons/typedlabeledontologyterm> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/simulation/src/main/resources/schemas/neurosciencegraph/simulation/bluepyefeconfiguration/v0.1.7.json" ;
    ns2:shapes <https://neuroshapes.org/dash/bluepyefeconfiguration/shapes/BluePyEfeConfigurationShape>,
        <https://neuroshapes.org/dash/bluepyefeconfiguration/shapes/ExperimentalCellShape> .

<https://neuroshapes.org/dash/bluepyefeconfiguration/shapes/BluePyEfeConfigurationShape> a ns1:NodeShape ;
    rdfs:label "BluePyEfe configuration shape"^^xsd:string ;
    rdfs:comment "BluePyEfe features extraction configuration shape."^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns1:property [ ns1:description "Optional list of experimental cells for this cell type" ;
                        ns1:node <https://neuroshapes.org/dash/bluepyefeconfiguration/shapes/ExperimentalCellShape> ;
                        ns1:path <https://neuroshapes.org/experimentalCell> ],
                    [ ns1:description "Stimuli to experiment mapping" ;
                        ns1:maxCount 1 ;
                        ns1:nodeKind ns1:BlankNode ;
                        ns1:path <https://neuroshapes.org/stimuliToExperimentMap> ],
                    [ ns1:description "Configuration dictionary for features extraction" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:nodeKind ns1:BlankNode ;
                        ns1:path <https://neuroshapes.org/featureExtractionConfiguration> ],
                    [ ns1:description "Electrical cell type" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:node <https://neuroshapes.org/commons/typedlabeledontologyterm/shapes/ElectricalCellTypeOntologyTermShape> ;
                        ns1:path <https://neuroshapes.org/eType> ],
                    [ ns1:description "Morphological cell type" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:node <https://neuroshapes.org/commons/typedlabeledontologyterm/shapes/MorphologicalCellTypeOntologyTermShape> ;
                        ns1:path <https://neuroshapes.org/mType> ],
                    [ ns1:description "Brain region" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Brain region" ;
                        ns1:node <https://neuroshapes.org/commons/typedlabeledontologyterm/shapes/BrainRegionOntologyTermShape> ;
                        ns1:path <https://neuroshapes.org/brainRegion> ],
                    [ ns1:description "Species" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Species" ;
                        ns1:node <https://neuroshapes.org/commons/typedlabeledontologyterm/shapes/SpeciesOntologyTermShape> ;
                        ns1:path <https://neuroshapes.org/species> ] ] ) ;
    ns1:targetClass <https://neuroshapes.org/Configuration> .

<https://neuroshapes.org/dash/bluepyefeconfiguration/shapes/ExperimentalCellShape> a ns1:NodeShape ;
    rdfs:label "Shape specifies cell list "^^xsd:string ;
    ns1:property [ ns1:datatype xsd:integer ;
            ns1:description "Channel number" ;
            ns1:maxCount 1 ;
            ns1:minCount 1 ;
            ns1:name "Channel" ;
            ns1:path <https://neuroshapes.org/channel> ],
        [ ns1:description "Comments about the cell" ;
            ns1:maxCount 1 ;
            ns1:path <http://schema.org/description> ],
        [ ns1:description "Name of the cell" ;
            ns1:maxCount 1 ;
            ns1:minCount 1 ;
            ns1:path <http://schema.org/name> ] .

