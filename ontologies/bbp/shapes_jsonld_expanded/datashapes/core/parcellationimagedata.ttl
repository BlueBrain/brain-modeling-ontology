<https://neuroshapes.org/dash/parcellationimagedata> a ns1:Schema ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/atlas/src/main/resources/schemas/neurosciencegraph/atlas/parcellationimagedata/v1.0.0.json" ;
