<https://neuroshapes.org/dash/parcellationimagedata/shapes/ParcellationImageDataShape> a ns2:NodeShape ;
    ns2:description "Parcellation image data" ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:targetClass <https://neuroshapes.org/ParcellationImageData> ;
    ns2:xone ( [ ns2:class <https://neuroshapes.org/ImageStack> ;
                ns2:node <https://neuroshapes.org/dash/imagestack/shapes/ImageStackShape> ] [ ns2:class <https://neuroshapes.org/ImageVolume> ;
                ns2:node <https://neuroshapes.org/dash/imagevolume/shapes/ImageVolumeShape> ] [ ns2:class <https://neuroshapes.org/DocumentAtlas> ;
                ns2:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] ) .

