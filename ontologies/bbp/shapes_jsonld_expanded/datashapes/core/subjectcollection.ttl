@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/subjectcollection> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/commons/subjectcollection> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/experiment/src/main/resources/schemas/neurosciencegraph/experiment/subjectcollection/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/subjectcollection/shapes/SubjectCollectionShape> .

<https://neuroshapes.org/dash/subjectcollection/shapes/SubjectCollectionShape> a ns1:NodeShape ;
    rdfs:label "Subject Collection"^^xsd:string ;
    rdfs:comment "All subjects in the collection should be of the same species."^^xsd:string ;
    ns1:node <https://neuroshapes.org/commons/subjectcollection/shapes/SubjectCollectionShape> ;
    ns1:targetClass <https://neuroshapes.org/SubjectCollection> .

