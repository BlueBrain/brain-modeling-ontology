@prefix ns1: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns2: <http://www.w3.org/ns/shacl#> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/transform> a ns1:Schema ;
    owl:imports <https://neuroshapes.org/commons/distribution>,
        <https://neuroshapes.org/commons/entity> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/atlas/src/main/resources/schemas/neurosciencegraph/atlas/transform/v0.2.0.json" ;
    ns1:shapes <https://neuroshapes.org/dash/transform/shapes/TransformShape> .

<https://neuroshapes.org/dash/transform/shapes/TransformShape> a ns2:NodeShape ;
    ns2:and ( [ ns2:node <https://neuroshapes.org/commons/entity/shapes/EntityShape> ] [ ns2:property [ ns2:description "Where the data attached to the entity is published." ;
                        ns2:minCount 1 ;
                        ns2:name "Distribution" ;
                        ns2:node <https://neuroshapes.org/commons/distribution/shapes/DistributionShape> ;
                        ns2:path <http://schema.org/distribution> ] ] ) ;
    ns2:description "Transform can be either linear or non-linear(deformable)." ;
    ns2:nodeKind ns2:BlankNodeOrIRI ;
    ns2:targetClass <https://neuroshapes.org/Transform> .

