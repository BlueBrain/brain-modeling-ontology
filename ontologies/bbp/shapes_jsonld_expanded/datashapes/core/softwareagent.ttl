@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/softwareagent> a ns2:Schema ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/core/src/main/resources/schemas/neurosciencegraph/core/softwareagent/v0.1.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/softwareagent/shapes/SoftwareAgentShape>,
        <https://neuroshapes.org/dash/softwareagent/shapes/SoftwareSourceCodeShape> .

<https://neuroshapes.org/dash/softwareagent/shapes/SoftwareAgentShape> a ns1:NodeShape ;
    ns1:description "Running software agent description." ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:property [ ns1:datatype xsd:string ;
            ns1:name "Software description" ;
            ns1:path <http://schema.org/description> ],
        [ ns1:class <http://schema.org/SoftwareSourceCode> ;
            ns1:description "The software source code." ;
            ns1:name "Software source code" ;
            ns1:node <https://neuroshapes.org/dash/softwareagent/shapes/SoftwareSourceCodeShape> ;
            ns1:path <https://neuroshapes.org/softwareSourceCode> ],
        [ ns1:datatype xsd:string ;
            ns1:name "Software name" ;
            ns1:path <http://schema.org/name> ],
        [ ns1:description "The configuration used with the software. This can be an IRI or the actual configuration in JSON." ;
            ns1:name "Software configuration" ;
            ns1:path <https://neuroshapes.org/configuration> ] ;
    ns1:targetClass ns3:SoftwareAgent .

<https://neuroshapes.org/dash/softwareagent/shapes/SoftwareSourceCodeShape> a ns1:NodeShape ;
    ns1:property [ ns1:datatype xsd:string ;
            ns1:description "The version of the CreativeWork embodied by a specified resource." ;
            ns1:name "Software version" ;
            ns1:path <http://schema.org/version> ],
        [ ns1:datatype xsd:string ;
            ns1:description "The computer programming language." ;
            ns1:name "Programming language" ;
            ns1:path <http://schema.org/programmingLanguage> ],
        [ ns1:datatype xsd:string ;
            ns1:description "Runtime platform or script interpreter dependencies (Example - Java v1, Python2.3, .Net Framework 3.0)." ;
            ns1:name "Runtime platform" ;
            ns1:path <http://schema.org/runtimePlatform> ],
        [ ns1:description "Link to the repository where the un-compiled, human readable code and related code is located (SVN, github, CodePlex)." ;
            ns1:name "Code repository" ;
            ns1:nodeKind ns1:IRI ;
            ns1:path <http://schema.org/codeRepository> ] .

