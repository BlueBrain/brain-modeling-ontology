@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix ns2: <https://bluebrain.github.io/nexus/vocabulary/> .
@prefix ns3: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://neuroshapes.org/dash/in_vitro_slice_wholecellpatchclamp_electrophysiology_trace> a ns2:Schema ;
    owl:imports <https://neuroshapes.org/dash/trace> ;
    ns3:wasDerivedFrom "https://github.com/INCF/neuroshapes/blob/v0.3.15/modules/electrophysiology/src/main/resources/schemas/neurosciencegraph/electrophysiology/trace/v1.0.0.json" ;
    ns2:shapes <https://neuroshapes.org/dash/in_vitro_slice_wholecellpatchclamp_electrophysiology_trace/shapes/TraceShape> .

<https://neuroshapes.org/dash/in_vitro_slice_wholecellpatchclamp_electrophysiology_trace/shapes/TraceShape> a ns1:NodeShape ;
    rdfs:label "Electrophysiology trace shape definition. A trace should have an IRI as identifier"^^xsd:string ;
    ns1:and ( [ ns1:node <https://neuroshapes.org/dash/trace/shapes/TraceShape> ] [ ns1:property [ ns1:class <https://neuroshapes.org/TraceGeneration> ;
                        ns1:description "Qualified trace generation" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Qualified generation" ;
                        ns1:path ns3:qualifiedGeneration ],
                    [ ns1:datatype xsd:integer ;
                        ns1:description "Channel number of trace" ;
                        ns1:maxCount 1 ;
                        ns1:minCount 1 ;
                        ns1:name "Channel" ;
                        ns1:path <https://neuroshapes.org/channel> ] ] ) ;
    ns1:nodeKind ns1:BlankNodeOrIRI ;
    ns1:targetClass <https://neuroshapes.org/InVitroSliceWholeCellPatchClampElectrophysiologyTrace>,
        <https://neuroshapes.org/Trace> .

