    rdfs:label "SSponNoHold"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:STesteCode a owl:Class ;
    rdfs:label "STesteCode"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:STrueNoise a owl:Class ;
    rdfs:label "STrueNoise"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SetAmpl a owl:Class ;
    rdfs:label "SetAmpl"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SetISI a owl:Class ;
