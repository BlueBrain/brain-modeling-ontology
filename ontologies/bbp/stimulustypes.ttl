    rdfs:subClassOf nsg:LNMCStimulus .

:SineSpec a owl:Class ;
    rdfs:label "SineSpec"@en ;
    rdfs:subClassOf nsg:BlueBrainStimulus ;
    skos:definition " For the SineSpec stimulus, current in the shape of a sine wave with increasing frequency is injected below firing threshold to assess how fast the cell can follow the change."@en .

:SpikeRec a owl:Class ;
    rdfs:label "SpikeRec"@en ;
    rdfs:subClassOf nsg:BlueBrainStimulus ;
    skos:definition "For the SpikeRec stimulus, a sequence of brief depolarizing currents with increasing interval is being injected, typically evoking first one and then two action potentials. The SpikeRec stimulus is used to assess the action potential recovery."@en .

:SpikeRec_Ih a owl:Class ;
    rdfs:label "SpikeRec_Ih"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SpikeRec_Kv1.1 a owl:Class ;
    rdfs:label "SpikeRec_Kv1.1"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SponAPs a owl:Class ;
    rdfs:label "SponAPs"@en ;
    rdfs:seeAlso :SpontAPs ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SponHold25 a owl:Class ;
    rdfs:label "SponHold25"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SponHold3 a owl:Class ;
    rdfs:label "SponHold3"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SponHold30 a owl:Class ;
    rdfs:label "SponHold30"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SponNoHold20 a owl:Class ;
    rdfs:label "SponNoHold20"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SponNoHold30 a owl:Class ;
    rdfs:label "SponNoHold30"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:Spontaneous a owl:Class ;
    rdfs:label "Spontaneous"@en ;
    rdfs:subClassOf nsg:BlueBrainStimulus ;
    skos:definition "Spontaneous recordings are used to assess input connectivity to other cells. No current is injected."@en .

:SsAHP a owl:Class ;
    rdfs:label "SsAHP"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:StartHold a owl:Class ;
    rdfs:label "StartHold"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:StartNoHold a owl:Class ;
    rdfs:label "StartNoHold"@en ;
