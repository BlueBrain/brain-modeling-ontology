:SAPWaveform a owl:Class ;
    rdfs:label "SAPWaveform"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SDelta a owl:Class ;
    rdfs:label "SDelta"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SIDRest a owl:Class ;
    rdfs:label "SIDRest"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SIDThres a owl:Class ;
    rdfs:label "SIDThres"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

:SIDTres a owl:Class ;
    rdfs:label "SIDTres"@en ;
    rdfs:subClassOf nsg:LNMCStimulus .

